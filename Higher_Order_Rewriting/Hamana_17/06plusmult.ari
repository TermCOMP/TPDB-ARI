(format higher-order)
(sort N)
(fun mult (-> N N N))
(fun plus (-> N N N))
(fun s (-> N N))
(fun z N)
(rule (plus z X) X)
(rule (plus (s Y) U) (plus Y (s U)))
(rule (plus (plus V W) P) (plus V (plus W P)))
(rule (mult z X1) z)
(rule (mult (s Y1) U1) (plus (mult Y1 U1) U1))
(rule (mult (plus V1 W1) P1) (plus (mult V1 P1) (mult W1 P1)))
