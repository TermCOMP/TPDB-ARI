(format higher-order)
(sort N)
(fun id (-> N N))
(fun succ (-> (-> (-> N N) N N) (-> N N) N N))
(fun two (-> (-> (-> (-> N N) N N) (-> N N) N N) (-> (-> N N) N N) (-> N N) N N))
(fun zero (-> (-> (-> (-> N N) N N) (-> N N) N N) (-> (-> N N) N N) (-> N N) N N))
(rule (succ F Z U) (F Z (Z U)))
(rule (id V) V)
(rule (two I J F Y) (I (I J) F Y))
(rule (zero F H I P) (H I P))
