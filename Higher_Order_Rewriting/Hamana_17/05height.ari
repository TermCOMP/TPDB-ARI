(format higher-order)
(sort N)
(sort f)
(sort t)
(fun cons (-> t f f))
(fun heightf (-> f N))
(fun heightt (-> t N))
(fun leaf t)
(fun max (-> N N N))
(fun nil f)
(fun node (-> f t))
(fun s (-> N N))
(fun z N)
(rule (heightf nil) z)
(rule (heightf (cons X Y)) (max (heightt X) (heightf Y)))
(rule (heightt leaf) z)
(rule (heightt (node U)) (s (heightf U)))
