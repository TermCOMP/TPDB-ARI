(format higher-order)
(sort a)
(sort aa)
(sort ta)
(fun abs (-> (-> a a) aa))
(fun app (-> aa a a))
(fun let (-> ta (-> a ta) ta))
(fun return (-> a ta))
(rule (app (abs F) Y) (F Y))
(rule (abs (lambda ((%Y a)) (app U %Y)) U)
(rule (let (return V) I) (I V))
(rule (let P (lambda ((%U a)) (return %U)) P)
(rule (let (let X1 Z1) G1) (let X1 (lambda ((%G a)) (let (Z1 %G) G1)))
