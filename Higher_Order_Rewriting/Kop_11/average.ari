(format higher-order)
(sort nat)
(fun O nat)
(fun apply (-> nat nat nat))
(fun avg (-> nat nat nat))
(fun check (-> nat nat))
(fun func (-> (-> nat nat) nat))
(fun s (-> nat nat))
(rule (avg (s X) Y) (avg X (s Y)))
(rule (avg U (s (s (s V)))) (s (avg (s U) V)))
(rule (avg O O) O)
(rule (avg O (s O)) O)
(rule (avg O (s (s O))) (s O))
(rule (apply (func I) P) (I (check P)))
(rule (check (s X1)) (s (check X1)))
(rule (check O) O)
