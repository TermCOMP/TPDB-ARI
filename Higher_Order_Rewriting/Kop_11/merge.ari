(format higher-order)
(sort list)
(sort nat)
(fun cons (-> nat list list))
(fun map (-> (-> nat nat) list list))
(fun merge (-> list list list list))
(fun nil list)
(rule (merge nil nil X) X)
(rule (merge nil (cons Y U) V) (merge U nil (cons Y V)))
(rule (merge (cons W P) X1 Y1) (merge X1 P (cons W Y1)))
(rule (map G1 nil) nil)
(rule (map H1 (cons W1 P1)) (cons (H1 W1) (map H1 P1)))
