(format higher-order)
(sort a)
(sort b)
(fun O b)
(fun cons (-> b a a))
(fun double (-> a a))
(fun inc (-> a a))
(fun map (-> (-> b b) a a))
(fun nil a)
(fun plus (-> b b b))
(fun s (-> b b))
(fun times (-> b b b))
(rule (plus O X) X)
(rule (plus (s Y) U) (s (plus Y U)))
(rule (times O V) O)
(rule (times (s W) P) (plus (times W P) P))
(rule (map F1 nil) nil)
(rule (map Z1 (cons U1 V1)) (cons (Z1 U1) (map Z1 V1)))
(rule inc (map (plus (s O))))
(rule double (map (times (s (s O)))))
