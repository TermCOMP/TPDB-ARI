(format higher-order)
(sort a)
(sort b)
(sort c)
(fun O (-> a b))
(fun @ (-> (-> (-> a b) a b) (-> a b) a b))
(fun cons (-> (-> a b) c c))
(fun hd (-> c a b))
(fun map (-> (-> (-> a b) a b) c c))
(fun nil c)
(rule (@ F O Y) (hd (map F (cons O nil)) Y))
(rule (map F nil) nil)
(rule (map F (cons I P)) (cons (@ F I) (map F P)))
(rule (@ F X) (F X))
