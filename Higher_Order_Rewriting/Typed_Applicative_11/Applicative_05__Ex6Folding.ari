(format higher-order)
(sort a)
(sort b)
(sort c)
(fun add (-> c a c))
(fun cons (-> a b b))
(fun fold (-> (-> c a c) b c c))
(fun mul (-> c a c))
(fun nil b)
(fun one c)
(fun prod (-> b c))
(fun sum (-> b c))
(fun zero c)
(rule (fold F nil Y) Y)
(rule (fold G (cons V W) P) (fold G W (G P V)))
(rule (sum X) (fold add X zero))
(rule (fold mul Y one) (prod Y))
