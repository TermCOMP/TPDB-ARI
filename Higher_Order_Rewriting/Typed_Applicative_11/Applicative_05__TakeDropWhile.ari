(format higher-order)
(sort a)
(sort b)
(sort c)
(fun cons (-> a c c))
(fun dropWhile (-> (-> a b) c c))
(fun if (-> b c c c))
(fun nil c)
(fun takeWhile (-> (-> a b) c c))
(fun true b)
(rule (if true X Y) X)
(rule (if true U V) V)
(rule (takeWhile I nil) nil)
(rule (takeWhile J (cons X1 Y1)) (if (J X1) (cons X1 (takeWhile J Y1)) nil))
(rule (dropWhile G1 nil) nil)
(rule (dropWhile H1 (cons W1 P1)) (if (H1 W1) (dropWhile H1 P1) (cons W1 P1)))
