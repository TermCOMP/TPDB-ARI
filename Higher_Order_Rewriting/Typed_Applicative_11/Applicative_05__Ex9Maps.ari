(format higher-order)
(sort a)
(sort b)
(sort c)
(sort d)
(sort e)
(fun cons (-> d e e))
(fun g b)
(fun map_1 (-> (-> d d) e e))
(fun map_2 (-> (-> d a d) a e e))
(fun map_3 (-> (-> b d c d) b c e e))
(rule (map_1 F (cons Y U)) (cons (F Y) (map_1 F U)))
(rule (map_2 I V (cons P X1)) (cons (I P V) (map_2 I V X1)))
(rule (map_3 G1 g Y1 (cons V1 W1)) (cons (G1 g V1 Y1) (map_3 G1 g Y1 W1)))
