(format higher-order)
(sort a)
(sort b)
(sort c)
(fun O c)
(fun cons (-> c b b))
(fun map (-> (-> c c) b b))
(fun nil b)
(fun node (-> a b c))
(fun plus (-> c c c))
(fun s (-> c c))
(fun size (-> c c))
(fun sum (-> b c))
(rule (map F nil) nil)
(rule (map Z (cons U V)) (cons (Z U) (map Z V)))
(rule (sum (cons W P)) (plus W (sum P)))
(rule (size (node X1 Y1)) (s (sum (map size Y1))))
(rule (plus O U1) O)
(rule (plus (s V1) W1) (s (plus V1 W1)))
