(format higher-order)
(sort a)
(sort b)
(sort c)
(fun cons (-> c b b))
(fun map (-> (-> c c) b b))
(fun nil b)
(fun node (-> a b c))
(fun treemap (-> (-> a a) c c))
(rule (map F nil) nil)
(rule (map Z (cons U V)) (cons (Z U) (map Z V)))
(rule (treemap I (node P X1)) (node (I P) (map (treemap I) X1)))
