(format higher-order)
(sort a)
(sort b)
(sort c)
(sort d)
(fun * (-> a a a))
(fun + (-> a a a))
(fun - (-> a a a))
(fun D (-> a a))
(fun cons (-> c d d))
(fun constant a)
(fun false b)
(fun filter (-> (-> c b) d d))
(fun filter2 (-> b (-> c b) c d d))
(fun map (-> (-> c c) d d))
(fun nil d)
(fun one a)
(fun t a)
(fun true b)
(fun zero a)
(rule (D t) one)
(rule (D constant) zero)
(rule (D (+ X Y)) (+ (D X) (D Y)))
(rule (D (* U V)) (+ (* V (D U)) (* U (D V))))
(rule (D (- W P)) (- (D W) (D P)))
(rule (map F1 nil) nil)
(rule (map Z1 (cons U1 V1)) (cons (Z1 U1) (map Z1 V1)))
(rule (filter I1 nil) nil)
(rule (filter J1 (cons X2 Y2)) (filter2 (J1 X2) J1 X2 Y2))
(rule (filter2 true G2 V2 W2) (cons V2 (filter G2 W2)))
(rule (filter2 false J2 X3 Y3) (filter J2 Y3))
