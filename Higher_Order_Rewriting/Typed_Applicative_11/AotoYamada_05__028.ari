(format higher-order)
(sort a)
(sort b)
(sort c)
(fun cons (-> a c c))
(fun consif (-> b a c c))
(fun false b)
(fun filter (-> (-> a b) c c))
(fun nil c)
(fun true b)
(rule (consif true X Y) (cons X Y))
(rule (consif false U V) V)
(rule (filter I nil) nil)
(rule (filter J (cons X1 Y1)) (consif (J X1) X1 (filter J Y1)))
