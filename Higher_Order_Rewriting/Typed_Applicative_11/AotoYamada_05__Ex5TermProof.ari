(format higher-order)
(sort a)
(fun O a)
(fun add (-> a a a))
(fun fact (-> a a))
(fun mult (-> a a a))
(fun rec (-> (-> a a a) a a a))
(fun s (-> a a))
(rule (add O X) X)
(rule (add (s Y) U) (s (add Y U)))
(rule (mult O V) O)
(rule (mult (s W) P) (add (mult W P) P))
(rule (rec F1 Y1 O) Y1)
(rule (rec G1 V1 (s W1)) (G1 (s W1) (rec G1 V1 W1)))
(rule fact (rec mult (s O)))
