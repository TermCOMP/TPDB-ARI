(format higher-order)
(sort a)
(sort b)
(sort c)
(fun cons (-> b c c))
(fun leaf (-> a b))
(fun mapt (-> (-> a a) b b))
(fun maptlist (-> (-> a a) c c))
(fun nil c)
(fun node (-> c b))
(rule (mapt F (leaf Y)) (leaf (F Y)))
(rule (mapt G (node V)) (node (maptlist G V)))
(rule (maptlist I nil) nil)
(rule (maptlist J (cons X1 Y1)) (cons (mapt J X1) (maptlist J Y1)))
