(format higher-order)
(sort a)
(sort b)
(sort c)
(fun cons (-> b c c))
(fun false a)
(fun filter (-> (-> b a) c c))
(fun filtersub (-> a (-> b a) c c))
(fun nil c)
(fun true a)
(rule (filter F nil) nil)
(rule (filter Z (cons U V)) (filtersub (Z U) Z (cons U V)))
(rule (filtersub true I (cons P X1)) (cons P (filter I X1)))
(rule (filtersub false Z1 (cons U1 V1)) (filter Z1 V1))
