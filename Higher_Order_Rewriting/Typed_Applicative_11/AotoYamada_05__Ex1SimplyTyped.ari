(format higher-order)
(sort a)
(sort b)
(sort c)
(fun O a)
(fun add (-> a a a))
(fun cons (-> b c c))
(fun id (-> a a))
(fun map (-> (-> b b) c c))
(fun nil c)
(fun s (-> a a))
(rule (id X) X)
(rule (add O) id)
(rule (add (s Y) U) (s (add Y U)))
(rule (map H nil) nil)
(rule (map I (cons P X1)) (cons (I P) (map I X1)))
