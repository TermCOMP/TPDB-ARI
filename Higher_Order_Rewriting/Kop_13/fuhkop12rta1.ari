(format higher-order)
(sort list)
(sort nat)
(fun app (-> list list list))
(fun cons (-> nat list list))
(fun hshuffle (-> (-> nat nat) list list))
(fun nil list)
(fun reverse (-> list list))
(rule (app nil X) X)
(rule (app (cons Y U) V) (cons Y (app U V)))
(rule (reverse nil) nil)
(rule (reverse (cons W P)) (app (reverse P) (cons W nil)))
(rule (hshuffle F1 nil) nil)
(rule (hshuffle Z1 (cons U1 V1)) (cons (Z1 U1) (hshuffle Z1 (reverse V1))))
