(format higher-order)
(sort nat)
(fun O nat)
(fun a (-> nat nat))
(fun f (-> (-> nat nat) nat nat nat))
(fun g (-> nat nat (-> nat nat) nat nat))
(fun h (-> nat nat))
(fun i (-> nat nat))
(fun s (-> nat nat))
(rule (f F Y U) (g Y U F Y))
(rule (g (s Y) (s U) F V) (g Y U F V))
(rule (g O O F V) (f F (s V) (F (h V))))
(rule (h O) (a O))
(rule (h (s Y)) (a (h Y)))
(rule (i O) O)
(rule (i (a Y)) (s (i Y)))
