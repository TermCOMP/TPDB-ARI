(format higher-order)
(sort N)
(fun dom (-> N N N N))
(fun eval (-> N N N))
(fun func (-> (-> N N) N N N))
(fun o N)
(fun s (-> N N))
(rule (eval (func F X Y) Z) (F (dom X Y Z)))
(rule (dom (s X) (s Y) (s Z)) (s (dom X Y Z)))
(rule (dom o (s Y) (s Z)) (s (dom o Y Z)))
(rule (dom X Y o) X)
(rule (dom o o Z) o)
