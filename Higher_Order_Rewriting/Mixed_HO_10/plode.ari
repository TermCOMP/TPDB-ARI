(format higher-order)
(sort list)
(sort nat)
(fun cons (-> nat list list))
(fun explode (-> list (-> nat nat) nat nat))
(fun implode (-> list (-> nat nat) nat nat))
(fun nil list)
(fun op (-> (-> nat nat) (-> nat nat) nat nat))
(rule (op F G X) (F (G X)))
(rule (implode nil F X) X)
(rule (implode (cons H T) F X) (implode T F (F X)))
(rule (explode nil F X) X)
(rule (explode (cons H T) F X) (explode T (op F F) (F X)))
