; @origtpdbfilename ./TRS/higher-order/AotoYam/026.trs
; @xtcfilename "./Derivational_Complexity_Innermost_Rewriting/AotoYamada_05/026.xml"
(format TRS)
(fun app 2)
(fun map 0)
(fun nil 0)
(fun cons 0)
(fun comp 0)
(fun twice 0)
(rule (app (app map f) nil) nil)
(rule (app (app map f) (app (app cons x) xs)) (app (app cons (app f x)) (app (app map f) xs)))
(rule (app (app (app comp f) g) x) (app f (app g x)))
(rule (app twice f) (app (app comp f) f))
