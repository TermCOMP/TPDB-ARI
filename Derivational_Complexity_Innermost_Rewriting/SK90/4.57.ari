; @origtpdbfilename ./TRS/SK90/4.57.trs
; @xtcfilename "./Derivational_Complexity_Innermost_Rewriting/SK90/4.57.xml"
(format TRS)
(fun f 3)
(fun g 4)
(fun <= 2)
(fun true 0)
(fun false 0)
(fun p 1)
(fun 0 0)
(fun s 1)
(rule (f x y z) (g (<= x y) x y z))
(rule (g true x y z) z)
(rule (g false x y z) (f (f (p x) y z) (f (p y) z x) (f (p z) x y)))
(rule (p 0) 0)
(rule (p (s x)) x)
