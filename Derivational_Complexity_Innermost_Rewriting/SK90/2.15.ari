(format TRS)
(fun f 1)
(fun |0| 0)
(fun |1| 0)
(fun s 1)
(fun g 1)
(fun + 2)
(rule (f |0|) |1|)
(rule (f (s x)) (g (f x)))
(rule (g x) (+ x (s x)))
(rule (f (s x)) (+ (f x) (s (f x))))
