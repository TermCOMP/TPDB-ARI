; @origtpdbfilename ./TRS/SK90/2.43.trs
; @xtcfilename "./Derivational_Complexity_Innermost_Rewriting/SK90/2.43.xml"
(format TRS)
(fun merge 2)
(fun nil 0)
(fun . 2)
(fun if 3)
(fun < 2)
(fun ++ 2)
(fun true 0)
(fun false 0)
(rule (merge nil y) y)
(rule (merge x nil) x)
(rule (merge (. x y) (. u v)) (if (< x u) (. x (merge y (. u v))) (. u (merge (. x y) v))))
(rule (++ nil y) y)
(rule (++ (. x y) z) (. x (++ y z)))
(rule (if true x y) x)
(rule (if false x y) x)
