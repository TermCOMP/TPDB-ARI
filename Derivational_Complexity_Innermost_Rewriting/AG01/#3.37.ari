(format TRS)
(fun not 1)
(fun true 0)
(fun false 0)
(fun evenodd 2)
(fun |0| 0)
(fun s 1)
(rule (not true) false)
(rule (not false) true)
(rule (evenodd x |0|) (not (evenodd x (s |0|))))
(rule (evenodd |0| (s |0|)) false)
(rule (evenodd (s x) (s |0|)) (evenodd x |0|))
