(format TRS)
(fun cond 2)
(fun true 0)
(fun odd 1)
(fun p 1)
(fun 0 0)
(fun false 0)
(fun s 1)
(rule (cond true x) (cond (odd x) (p (p (p x)))))
(rule (odd 0) false)
(rule (odd (s 0)) true)
(rule (odd (s (s x))) (odd x))
(rule (p 0) 0)
(rule (p (s x)) x)
