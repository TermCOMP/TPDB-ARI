; @origtpdbfilename ./TRS/Beerendonk/3.trs
; @xtcfilename "./Derivational_Complexity_Innermost_Rewriting/Beerendonk_07/3.xml"
(format TRS)
(fun cond 3)
(fun true 0)
(fun gr 2)
(fun add 2)
(fun |0| 0)
(fun false 0)
(fun s 1)
(rule (cond true x y) (cond (gr x y) x (add x y)))
(rule (gr |0| x) false)
(rule (gr (s x) |0|) true)
(rule (gr (s x) (s y)) (gr x y))
(rule (add |0| x) x)
(rule (add (s x) y) (s (add x y)))
