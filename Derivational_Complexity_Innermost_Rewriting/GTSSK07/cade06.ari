; @origtpdbfilename ./TRS/SchneiderKamp/trs/cade06.trs
; @xtcfilename "./Derivational_Complexity_Innermost_Rewriting/GTSSK07/cade06.xml"
(format TRS)
(fun f 4)
(fun true 0)
(fun gt 2)
(fun plus 2)
(fun s 1)
(fun 0 0)
(fun false 0)
(rule (f true x y z) (f (gt x (plus y z)) x (s y) z))
(rule (f true x y z) (f (gt x (plus y z)) x y (s z)))
(rule (plus n 0) n)
(rule (plus n (s m)) (s (plus n m)))
(rule (gt 0 v) false)
(rule (gt (s u) 0) true)
(rule (gt (s u) (s v)) (gt u v))
