; @origtpdbfilename ./TRS/SchneiderKamp/trs/cade07.trs
; @xtcfilename "./Derivational_Complexity_Innermost_Rewriting/GTSSK07/cade07.xml"
(format TRS)
(fun f 4)
(fun true 0)
(fun g 4)
(fun gt 2)
(fun s 1)
(fun |0| 0)
(fun false 0)
(rule (f true x y z) (g (gt x y) x y z))
(rule (g true x y z) (f (gt x z) x (s y) z))
(rule (g true x y z) (f (gt x z) x y (s z)))
(rule (gt |0| v) false)
(rule (gt (s u) |0|) true)
(rule (gt (s u) (s v)) (gt u v))
