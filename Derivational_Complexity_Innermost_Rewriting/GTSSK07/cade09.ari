; @origtpdbfilename ./TRS/SchneiderKamp/trs/cade09.trs
; @xtcfilename "./Derivational_Complexity_Innermost_Rewriting/GTSSK07/cade09.xml"
(format TRS)
(fun f 3)
(fun true 0)
(fun gt 2)
(fun round 1)
(fun s 1)
(fun 0 0)
(fun false 0)
(rule (f true x y) (f (gt x y) x (round (s y))))
(rule (round 0) 0)
(rule (round (s 0)) (s (s 0)))
(rule (round (s (s x))) (s (s (round x))))
(rule (gt 0 v) false)
(rule (gt (s u) 0) true)
(rule (gt (s u) (s v)) (gt u v))
