; @origtpdbfilename TRS//AProVE_10/ex1.trs
; @xtcfilename "./Derivational_Complexity_Innermost_Rewriting/AProVE_10/ex1.xml"
(format TRS)
(fun f 2)
(fun cond 3)
(fun lt 2)
(fun tt 0)
(fun s 1)
(fun 0 0)
(rule (f x y) (cond (lt x y) x y))
(rule (cond tt x y) (f (s x) (s y)))
(rule (lt 0 y) tt)
(rule (lt (s x) (s y)) (lt x y))
