(format TRS)
(fun table 0)
(fun gen 1)
(fun s 1)
(fun 0 0)
(fun if1 2)
(fun le 2)
(fun 10 0)
(fun false 0)
(fun nil 0)
(fun true 0)
(fun if2 2)
(fun if3 3)
(fun cons 2)
(fun entry 3)
(fun times 2)
(fun plus 2)
(rule table (gen (s 0)))
(rule (gen x) (if1 (le x 10) x))
(rule (if1 false x) nil)
(rule (if1 true x) (if2 x x))
