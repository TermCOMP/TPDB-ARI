(rule (le (s x) 0) false)
(rule (le (s x) (s y)) (le x y))
(rule (plus 0 y) y)
(rule (plus (s x) y) (s (plus x y)))
(rule (times 0 y) 0)
(rule (times (s x) y) (plus y (times x y)))
(rule 10 (s (s (s (s (s (s (s (s (s (s 0)))))))))))
