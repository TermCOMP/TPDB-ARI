; @origtpdbfilename ./TRS/SchneiderKamp/trs/thiemann27.trs
; @xtcfilename "./Derivational_Complexity_Innermost_Rewriting/AProVE_07/thiemann27.xml"
(format TRS)
(fun le 2)
(fun |0| 0)
(fun true 0)
(fun s 1)
(fun false 0)
(fun int 2)
(fun if 3)
(fun cons 2)
(fun nil 0)
(rule (le |0| y) true)
(rule (le (s x) |0|) false)
(rule (le (s x) (s y)) (le x y))
(rule (int x y) (if (le x y) x y))
(rule (if true x y) (cons x (int (s x) y)))
(rule (if false x y) nil)
