; @origtpdbfilename ./TRS/SchneiderKamp/trs/otto03.trs
; @xtcfilename "./Derivational_Complexity_Innermost_Rewriting/AProVE_07/otto03.xml"
(format TRS)
(fun lt 2)
(fun |0| 0)
(fun s 1)
(fun true 0)
(fun false 0)
(fun fac 1)
(fun help 2)
(fun if 3)
(fun times 2)
(rule (lt |0| (s x)) true)
(rule (lt x |0|) false)
(rule (lt (s x) (s y)) (lt x y))
(rule (fac x) (help x |0|))
(rule (help x c) (if (lt c x) x c))
(rule (if true x c) (times (s c) (help x (s c))))
(rule (if false x c) (s |0|))
