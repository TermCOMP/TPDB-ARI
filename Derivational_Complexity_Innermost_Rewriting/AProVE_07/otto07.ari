; @origtpdbfilename ./TRS/SchneiderKamp/trs/otto07.trs
; @xtcfilename "./Derivational_Complexity_Innermost_Rewriting/AProVE_07/otto07.xml"
(format TRS)
(fun lt 2)
(fun |0| 0)
(fun s 1)
(fun true 0)
(fun false 0)
(fun minus 2)
(fun help 3)
(rule (lt |0| (s x)) true)
(rule (lt x |0|) false)
(rule (lt (s x) (s y)) (lt x y))
(rule (minus x y) (help (lt y x) x y))
(rule (help true x y) (s (minus x (s y))))
(rule (help false x y) |0|)
