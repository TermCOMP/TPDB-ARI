(rule (active (add X1 X2)) (add (active X1) X2))
(rule (active (add X1 X2)) (add X1 (active X2)))
(rule (active (len X)) (len (active X)))
(rule (cons (mark X1) X2) (mark (cons X1 X2)))
(rule (fst (mark X1) X2) (mark (fst X1 X2)))
