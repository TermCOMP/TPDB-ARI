(format TRS)
(fun active 1)
(fun fst 2)
(fun 0 0)
(fun mark 1)
(fun nil 0)
(fun s 1)
(fun cons 2)
(fun from 1)
(fun add 2)
(fun len 1)
(fun proper 1)
(fun ok 1)
(fun top 1)
(rule (active (fst 0 Z)) (mark nil))
(rule (active (fst (s X) (cons Y Z))) (mark (cons Y (fst X Z))))
(rule (active (from X)) (mark (cons X (from (s X)))))
