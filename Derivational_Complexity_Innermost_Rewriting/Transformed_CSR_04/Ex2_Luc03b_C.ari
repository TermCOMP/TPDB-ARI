(rule (cons (ok X1) (ok X2)) (ok (cons X1 X2)))
(rule (fst (ok X1) (ok X2)) (ok (fst X1 X2)))
(rule (from (ok X)) (ok (from X)))
(rule (add (ok X1) (ok X2)) (ok (add X1 X2)))
