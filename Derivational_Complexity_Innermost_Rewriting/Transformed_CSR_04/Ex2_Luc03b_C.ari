(rule (active (len (cons X Z))) (mark (s (len Z))))
(rule (active (cons X1 X2)) (cons (active X1) X2))
(rule (active (fst X1 X2)) (fst (active X1) X2))
(rule (active (fst X1 X2)) (fst X1 (active X2)))
