; @origtpdbfilename ./TRS/TRCSR/Ex5_Zan97_Z.trs
; @xtcfilename "./Derivational_Complexity_Innermost_Rewriting/Transformed_CSR_04/Ex5_Zan97_Z.xml"
(format TRS)
(fun f 1)
(fun if 3)
(fun c 0)
(fun n__f 1)
(fun true 0)
(fun false 0)
(fun activate 1)
(rule (f X) (if X c (n__f true)))
(rule (if true X Y) X)
(rule (if false X Y) (activate Y))
(rule (f X) (n__f X))
(rule (activate (n__f X)) (f X))
(rule (activate X) X)
