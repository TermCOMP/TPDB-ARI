(format TRS)
(fun f 1)
(fun n__f 1)
(fun n__a 0)
(fun n__g 1)
(fun a 0)
(fun g 1)
(fun activate 1)
(rule (f (n__f n__a)) (f (n__g (n__f n__a))))
(rule (f X) (n__f X))
