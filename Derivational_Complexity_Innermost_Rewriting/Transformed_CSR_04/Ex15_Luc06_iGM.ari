; @xtcfilename "./Derivational_Complexity_Innermost_Rewriting/Transformed_CSR_04/Ex15_Luc06_iGM.xml"
(format TRS)
(fun active 1)
(fun f 1)
(fun a 0)
(fun mark 1)
(fun g 1)
(rule (active (f (f a))) (mark (f (g (f a)))))
(rule (mark (f X)) (active (f X)))
(rule (mark a) (active a))
(rule (mark (g X)) (active (g (mark X))))
(rule (f (mark X)) (f X))
(rule (f (active X)) (f X))
(rule (g (mark X)) (g X))
(rule (g (active X)) (g X))
