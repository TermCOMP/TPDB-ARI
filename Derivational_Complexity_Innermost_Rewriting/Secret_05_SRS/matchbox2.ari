(format TRS)
(fun t 1)
(fun o 1)
(fun m 1)
(fun a 1)
(fun e 1)
(fun n 1)
(fun s 1)
(fun l 1)
(rule (t (o x1)) (m (a x1)))
(rule (t (e x1)) (n (s x1)))
(rule (a (l x1)) (a (t x1)))
(rule (o (m (a x1))) (t (e (n x1))))
(rule (s (a x1)) (l (a (t (o (m (a (t (e x1)))))))))
(rule (n (s x1)) (a (l (a (t x1)))))
