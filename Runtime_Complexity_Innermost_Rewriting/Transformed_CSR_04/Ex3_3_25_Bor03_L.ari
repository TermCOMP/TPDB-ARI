(format TRS)
(fun app 2)
(fun nil 0)
(fun cons 1)
(fun from 1)
(fun zWadr 2)
(fun prefix 1)
(rule (app nil YS) YS)
(rule (app (cons X) YS) (cons X))
(rule (from X) (cons X))
