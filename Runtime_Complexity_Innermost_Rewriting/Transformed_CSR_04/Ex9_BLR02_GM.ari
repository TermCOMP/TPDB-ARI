(rule (mark (cons X1 X2)) (cons (mark X1) X2))
(rule (mark |0|) |0|)
(rule (mark (s X)) (s (mark X)))
(rule (a__filter X1 X2 X3) (filter X1 X2 X3))
(rule (a__sieve X) (sieve X))
(rule (a__nats X) (nats X))
(rule a__zprimes zprimes)
