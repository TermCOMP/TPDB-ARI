(format TRS)
(fun a__filter 3)
(fun cons 2)
(fun |0| 0)
(fun filter 3)
(fun s 1)
(fun mark 1)
(fun a__sieve 1)
(fun sieve 1)
(fun a__nats 1)
(fun nats 1)
(fun a__zprimes 0)
(fun zprimes 0)
(rule (a__filter (cons X Y) |0| M) (cons |0| (filter Y M M)))
(rule (a__filter (cons X Y) (s N) M) (cons (mark X) (filter Y N M)))
(rule (a__sieve (cons |0| Y)) (cons |0| (sieve Y)))
(rule (a__sieve (cons (s N) Y)) (cons (s (mark N)) (sieve (filter Y N N))))
