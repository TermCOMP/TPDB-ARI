; @origtpdbfilename ./TRS/SK90/2.45.trs
; @xtcfilename "./Runtime_Complexity_Innermost_Rewriting/SK90/2.45.xml"
(format TRS)
(fun admit 2)
(fun nil 0)
(fun . 2)
(fun w 0)
(fun cond 2)
(fun = 2)
(fun sum 3)
(fun carry 3)
(fun true 0)
(rule (admit x nil) nil)
(rule (admit x (. u (. v (. w z)))) (cond (= (sum x u v) w) (. u (. v (. w (admit (carry x u v) z))))))
(rule (cond true y) y)
