; @xtcfilename "./Runtime_Complexity_Innermost_Rewriting/SK90/4.13.xml"
(format TRS)
(fun - 2)
(fun 0 0)
(fun s 1)
(fun if 3)
(fun greater 2)
(fun p 1)
(rule (- 0 y) 0)
(rule (- x 0) x)
(rule (- x (s y)) (if (greater x (s y)) (s (- x (p (s y)))) 0))
(rule (p 0) 0)
(rule (p (s x)) x)
