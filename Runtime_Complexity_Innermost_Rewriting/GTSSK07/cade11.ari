; @origtpdbfilename ./TRS/SchneiderKamp/trs/cade11.trs
; @xtcfilename "./Runtime_Complexity_Innermost_Rewriting/GTSSK07/cade11.xml"
(format TRS)
(fun f 3)
(fun true 0)
(fun gt 2)
(fun s 1)
(fun 0 0)
(fun false 0)
(rule (f true x y) (f (gt x y) (s x) (s (s y))))
(rule (gt 0 v) false)
(rule (gt (s u) 0) true)
(rule (gt (s u) (s v)) (gt u v))
