; @origtpdbfilename ./TRS/SchneiderKamp/trs/cade03.trs
; @xtcfilename "./Runtime_Complexity_Innermost_Rewriting/GTSSK07/cade03.xml"
(format TRS)
(fun minus 2)
(fun cond 3)
(fun ge 2)
(fun s 1)
(fun false 0)
(fun 0 0)
(fun true 0)
(rule (minus x y) (cond (ge x (s y)) x y))
(rule (cond false x y) 0)
(rule (cond true x y) (s (minus x (s y))))
(rule (ge u 0) true)
(rule (ge 0 (s v)) false)
(rule (ge (s u) (s v)) (ge u v))
