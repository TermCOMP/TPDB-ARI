; @origtpdbfilename ./TRS/AG01/#3.18.trs
; @xtcfilename "./Runtime_Complexity_Innermost_Rewriting/AG01/#3.18.xml"
(format TRS)
(fun minus 2)
(fun 0 0)
(fun s 1)
(fun double 1)
(fun plus 2)
(rule (minus x 0) x)
(rule (minus (s x) (s y)) (minus x y))
(rule (double 0) 0)
(rule (double (s x)) (s (s (double x))))
(rule (plus 0 y) y)
(rule (plus (s x) y) (s (plus x y)))
(rule (plus (s x) y) (plus x (s y)))
(rule (plus (s x) y) (s (plus (minus x y) (double y))))
