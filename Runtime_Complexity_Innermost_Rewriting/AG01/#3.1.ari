; @origtpdbfilename ./TRS/AG01/#3.1.trs
; @xtcfilename "./Runtime_Complexity_Innermost_Rewriting/AG01/#3.1.xml"
(format TRS)
(fun minus 2)
(fun 0 0)
(fun s 1)
(fun quot 2)
(rule (minus x 0) x)
(rule (minus (s x) (s y)) (minus x y))
(rule (quot 0 (s y)) 0)
(rule (quot (s x) (s y)) (s (quot (minus x y) (s y))))
