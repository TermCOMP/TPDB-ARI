(format TRS)
(fun f 1)
(fun cons 2)
(fun g 1)
(fun 0 0)
(fun s 1)
(fun sel 2)
(rule (f X) (cons X (f (g X))))
(rule (g 0) (s 0))
(rule (g (s X)) (s (s (g X))))
(rule (sel 0 (cons X Y)) X)
(rule (sel (s X) (cons Y Z)) (sel X Z))
