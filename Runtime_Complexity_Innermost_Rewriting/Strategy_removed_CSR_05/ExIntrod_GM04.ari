(format TRS)
(fun nats 0)
(fun adx 1)
(fun zeros 0)
(fun cons 2)
(fun |0| 0)
(fun incr 1)
(fun s 1)
(fun hd 1)
(fun tl 1)
(rule nats (adx zeros))
(rule zeros (cons |0| zeros))
(rule (incr (cons X Y)) (cons (s X) (incr Y)))
(rule (adx (cons X Y)) (incr (cons X (adx Y))))
(rule (hd (cons X Y)) X)
(rule (tl (cons X Y)) Y)
