; @origtpdbfilename ./TRS/Beerendonk/1.trs
; @xtcfilename "./Runtime_Complexity_Innermost_Rewriting/Beerendonk_07/1.xml"
(format TRS)
(fun cond 3)
(fun true 0)
(fun gr 2)
(fun p 1)
(fun 0 0)
(fun false 0)
(fun s 1)
(rule (cond true x y) (cond (gr x y) (p x) y))
(rule (gr 0 x) false)
(rule (gr (s x) 0) true)
(rule (gr (s x) (s y)) (gr x y))
(rule (p 0) 0)
(rule (p (s x)) x)
