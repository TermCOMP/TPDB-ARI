(format TRS)
(fun f 3)
(fun |0| 0)
(fun |1| 0)
(fun g 1)
(rule (f |0| |1| x) (f (g x) (g x) x))
(rule (f (g x) y z) (g (f x y z)))
(rule (f x (g y) z) (g (f x y z)))
(rule (f x y (g z)) (g (f x y z)))
