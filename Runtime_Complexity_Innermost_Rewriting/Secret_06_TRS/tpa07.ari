; @xtcfilename "./Runtime_Complexity_Innermost_Rewriting/Secret_06_TRS/tpa07.xml"
(format TRS)
(fun - 2)
(fun |0| 0)
(fun s 1)
(fun + 2)
(fun * 2)
(fun f 2)
(rule (- x |0|) x)
(rule (- (s x) (s y)) (- x y))
(rule (+ |0| y) y)
(rule (+ (s x) y) (s (+ x y)))
(rule (* x |0|) |0|)
(rule (* x (s y)) (+ x (* x y)))
(rule (f (s x) y) (f (- (* (s x) (s y)) (s (* (s x) y))) (* y y)))
