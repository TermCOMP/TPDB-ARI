; @xtcfilename "./Runtime_Complexity_Innermost_Rewriting/Frederiksen_Glenstrup/fold.xml"
(format TRS)
(fun foldl 2)
(fun Cons 2)
(fun S 1)
(fun |0| 0)
(fun foldr 2)
(fun op 2)
(fun Nil 0)
(fun notEmpty 1)
(fun True 0)
(fun False 0)
(fun fold 2)
(rule (foldl x (Cons (S |0|) xs)) (foldl (S x) xs))
(rule (foldl (S |0|) (Cons x xs)) (foldl (S x) xs))
(rule (foldr a (Cons x xs)) (op x (foldr a xs)))
(rule (foldr a Nil) a)
(rule (foldl a Nil) a)
(rule (notEmpty (Cons x xs)) True)
(rule (notEmpty Nil) False)
(rule (op x (S |0|)) (S x))
(rule (op (S |0|) y) (S y))
(rule (fold a xs) (Cons (foldl a xs) (Cons (foldr a xs) Nil)))
