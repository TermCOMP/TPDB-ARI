; @xtcfilename "./Runtime_Complexity_Innermost_Rewriting/Frederiksen_Glenstrup/mul_better.xml"
(format TRS)
(fun mul0 2)
(fun C 2)
(fun add0 2)
(fun Z 0)
(fun S 0)
(fun second 1)
(fun isZero 1)
(fun False 0)
(fun True 0)
(fun goal 2)
(rule (mul0 (C x y) y') (add0 (mul0 y y') y'))
(rule (mul0 Z y) Z)
(rule (add0 (C x y) y') (add0 y (C S y')))
(rule (add0 Z y) y)
(rule (second (C x y)) y)
(rule (isZero (C x y)) False)
(rule (isZero Z) True)
(rule (goal xs ys) (mul0 xs ys))
