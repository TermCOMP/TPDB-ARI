; @xtcfilename "./Runtime_Complexity_Innermost_Rewriting/Frederiksen_Glenstrup/mul.xml"
(format TRS)
(fun mul0 2)
(fun Cons 2)
(fun add0 2)
(fun S 0)
(fun Nil 0)
(fun goal 2)
(rule (mul0 (Cons x xs) y) (add0 (mul0 xs y) y))
(rule (add0 (Cons x xs) y) (add0 xs (Cons S y)))
(rule (mul0 Nil y) Nil)
(rule (add0 Nil y) y)
(rule (goal xs ys) (mul0 xs ys))
