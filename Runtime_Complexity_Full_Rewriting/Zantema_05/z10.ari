; @origtpdbfilename ./TRS/Zantema/z10.trs
; @xtcfilename "./Runtime_Complexity_Full_Rewriting/Zantema_05/z10.xml"
(format TRS)
(fun a 2)
(fun lambda 1)
(fun p 2)
(fun |1| 0)
(fun t 0)
(fun id 0)
(rule (a (lambda x) y) (lambda (a x (p |1| (a y t)))))
(rule (a (p x y) z) (p (a x z) (a y z)))
(rule (a (a x y) z) (a x (a y z)))
(rule (a id x) x)
(rule (a |1| id) |1|)
(rule (a t id) t)
(rule (a |1| (p x y)) x)
(rule (a t (p x y)) y)
