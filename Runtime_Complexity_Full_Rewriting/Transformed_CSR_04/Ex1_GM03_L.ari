; @origtpdbfilename ./TRS/TRCSR/Ex1_GM03_L.trs
; @xtcfilename "./Runtime_Complexity_Full_Rewriting/Transformed_CSR_04/Ex1_GM03_L.xml"
(format TRS)
(fun if 1)
(fun true 0)
(fun false 0)
(fun p 1)
(fun 0 0)
(fun s 1)
(fun leq 2)
(fun diff 2)
(rule (if true) X)
(rule (if false) Y)
(rule (p 0) 0)
(rule (p (s X)) X)
(rule (leq 0 Y) true)
(rule (leq (s X) 0) false)
(rule (leq (s X) (s Y)) (leq X Y))
(rule (diff X Y) (if (leq X Y)))
