; @xtcfilename "./Runtime_Complexity_Full_Rewriting/Transformed_CSR_04/MYNAT_nosorts_FR.xml"
(format TRS)
(fun and 2)
(fun tt 0)
(fun activate 1)
(fun plus 2)
(fun 0 0)
(fun s 1)
(fun x 2)
(rule (and tt X) (activate X))
(rule (plus N 0) N)
(rule (plus N (s M)) (s (plus N M)))
(rule (x N 0) 0)
(rule (x N (s M)) (plus (x N M) N))
(rule (activate X) X)
