(rule a__zeros (cons |0| zeros))
(rule (a__U11 tt L) (a__U12 tt L))
(rule (a__U12 tt L) (s (a__length (mark L))))
(rule (a__U21 tt IL M N) (a__U22 tt IL M N))
(rule (a__U22 tt IL M N) (a__U23 tt IL M N))
