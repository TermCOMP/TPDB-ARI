(format TRS)
(fun a__zeros 0)
(fun cons 2)
(fun |0| 0)
(fun zeros 0)
(fun a__U11 2)
(fun tt 0)
(fun a__U12 2)
(fun s 1)
(fun a__length 1)
(fun mark 1)
(fun a__U21 4)
(fun a__U22 4)
(fun a__U23 4)
