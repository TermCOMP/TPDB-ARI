; @origtpdbfilename ./TRS/SK90/2.41.trs
; @xtcfilename "./Runtime_Complexity_Full_Rewriting/SK90/2.41.xml"
(format TRS)
(fun norm 1)
(fun nil 0)
(fun 0 0)
(fun g 2)
(fun s 1)
(fun f 2)
(fun rem 2)
(rule (norm nil) 0)
(rule (norm (g x y)) (s (norm x)))
(rule (f x nil) (g nil x))
(rule (f x (g y z)) (g (f x y) z))
(rule (rem nil y) nil)
(rule (rem (g x y) 0) (g x y))
(rule (rem (g x y) (s z)) (rem x z))
