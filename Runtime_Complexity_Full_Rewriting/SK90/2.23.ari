; @origtpdbfilename ./TRS/SK90/2.23.trs
; @xtcfilename "./Runtime_Complexity_Full_Rewriting/SK90/2.23.xml"
(format TRS)
(fun fac 1)
(fun 0 0)
(fun 1 0)
(fun s 1)
(fun * 2)
(fun floop 2)
(fun + 2)
(rule (fac 0) 1)
(rule (fac (s x)) (* (s x) (fac x)))
(rule (floop 0 y) y)
(rule (floop (s x) y) (floop x (* (s x) y)))
(rule (* x 0) 0)
(rule (* x (s y)) (+ (* x y) x))
(rule (+ x 0) x)
(rule (+ x (s y)) (s (+ x y)))
(rule 1 (s 0))
(rule (fac 0) (s 0))
