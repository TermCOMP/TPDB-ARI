(format TRS)
(fun fst 2)
(fun 0 0)
(fun nil 0)
(fun s 1)
(fun cons 2)
(fun from 1)
(fun add 2)
(fun len 1)
(rule (fst 0 Z) nil)
(rule (fst (s X) (cons Y Z)) (cons Y (fst X Z)))
(rule (from X) (cons X (from (s X))))
(rule (add 0 X) X)
(rule (add (s X) Y) (s (add X Y)))
(rule (len nil) 0)
(rule (len (cons X Z)) (s (len Z)))
