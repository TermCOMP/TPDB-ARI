(format TRS)
(fun f 1)
(fun |0| 0)
(fun true 0)
(fun |1| 0)
(fun false 0)
(fun s 1)
(fun if 3)
(fun g 2)
(fun c 1)
(rule (f |0|) true)
(rule (f |1|) false)
(rule (f (s x)) (f x))
(rule (if true (s x) (s y)) (s x))
(rule (if false (s x) (s y)) (s y))
(rule (g x (c y)) (c (g x y)))
(rule (g x (c y)) (g x (if (f x) (c (g (s x) y)) (c y))))
