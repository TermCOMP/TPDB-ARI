(format TRS)
(fun minus 2)
(fun 0 0)
(fun s 1)
(fun f 1)
(fun g 1)
(rule (minus x 0) x)
(rule (minus (s x) (s y)) (minus x y))
(rule (f 0) (s 0))
(rule (f (s x)) (minus (s x) (g (f x))))
(rule (g 0) 0)
(rule (g (s x)) (minus (s x) (f (g x))))
