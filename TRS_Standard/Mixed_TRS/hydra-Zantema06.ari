; @origtpdbfilename ./TRS/Zantema06/hydra.trs
; @xtcfilename "./TRS_Standard/Mixed_TRS/hydra-Zantema06.xml"
(format TRS)
(fun h 2)
(fun e 1)
(fun d 2)
(fun s 1)
(fun g 2)
(fun 0 0)
(rule (h (e x) y) (h (d x y) (s y)))
(rule (d (g (g 0 x) y) (s z)) (g (e x) (d (g (g 0 x) y) z)))
(rule (d (g (g 0 x) y) 0) (e y))
(rule (d (g 0 x) y) (e x))
(rule (d (g x y) z) (g (d x z) (e y)))
(rule (g (e x) (e y)) (e (g x y)))
