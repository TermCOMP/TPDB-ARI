; @origtpdbfilename ./TRS/SK90/2.19.trs
; @xtcfilename "./TRS_Standard/SK90/2.19.xml"
(format TRS)
(fun sqr 1)
(fun |0| 0)
(fun s 1)
(fun + 2)
(fun double 1)
(rule (sqr |0|) |0|)
(rule (sqr (s x)) (+ (sqr x) (s (double x))))
(rule (double |0|) |0|)
(rule (double (s x)) (s (s (double x))))
(rule (+ x |0|) x)
(rule (+ x (s y)) (s (+ x y)))
(rule (sqr (s x)) (s (+ (sqr x) (double x))))
