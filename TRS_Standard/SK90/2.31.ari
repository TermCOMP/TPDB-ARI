; @xtcfilename "./TRS_Standard/SK90/2.31.xml"
(format TRS)
(fun not 1)
(fun true 0)
(fun false 0)
(fun odd 1)
(fun 0 0)
(fun s 1)
(fun + 2)
(rule (not true) false)
(rule (not false) true)
(rule (odd 0) false)
(rule (odd (s x)) (not (odd x)))
(rule (+ x 0) x)
(rule (+ x (s y)) (s (+ x y)))
(rule (+ (s x) y) (s (+ x y)))
