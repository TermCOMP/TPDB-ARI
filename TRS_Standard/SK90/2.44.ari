; @origtpdbfilename ./TRS/SK90/2.44.trs
; @xtcfilename "./TRS_Standard/SK90/2.44.xml"
(format TRS)
(fun del 1)
(fun . 2)
(fun f 4)
(fun = 2)
(fun true 0)
(fun false 0)
(fun nil 0)
(fun u 0)
(fun v 0)
(fun and 2)
(rule (del (. x (. y z))) (f (= x y) x y z))
(rule (f true x y z) (del (. y z)))
(rule (f false x y z) (. x (del (. y z))))
(rule (= nil nil) true)
(rule (= (. x y) nil) false)
(rule (= nil (. y z)) false)
(rule (= (. x y) (. u v)) (and (= x u) (= y v)))
