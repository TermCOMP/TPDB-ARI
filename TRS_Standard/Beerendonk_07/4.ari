; @xtcfilename "./TRS_Standard/Beerendonk_07/4.xml"
(format TRS)
(fun cond 3)
(fun true 0)
(fun gr 2)
(fun 0 0)
(fun false 0)
(fun s 1)
(rule (cond true x y) (cond (gr x y) y x))
(rule (gr 0 x) false)
(rule (gr (s x) 0) true)
(rule (gr (s x) (s y)) (gr x y))
