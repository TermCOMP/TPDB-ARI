; @origtpdbfilename ./TRS/Beerendonk/2.trs
; @xtcfilename "./TRS_Standard/Beerendonk_07/2.xml"
(format TRS)
(fun cond 3)
(fun true 0)
(fun gr 2)
(fun p 1)
(fun s 1)
(fun |0| 0)
(fun false 0)
(rule (cond true x y) (cond (gr x y) (p x) (s y)))
(rule (gr |0| x) false)
(rule (gr (s x) |0|) true)
(rule (gr (s x) (s y)) (gr x y))
(rule (p |0|) |0|)
(rule (p (s x)) x)
