(format TRS)
(fun app 2)
(fun if 0)
(fun true 0)
(fun false 0)
(fun filter 0)
(fun nil 0)
(fun cons 0)
(rule (app (app (app if true) x) y) x)
(rule (app (app (app if false) x) y) y)
(rule (app (app filter f) nil) nil)
(rule (app (app filter f) (app (app cons x) xs)) (app (app (app if (app f x)) (app (app cons x) (app (app filter f) xs))) (app (app filter f) xs)))
