; @xtcfilename "./TRS_Standard/AotoYamada_05/Ex1SimplyTyped.xml"
(format TRS)
(fun app 2)
(fun id 0)
(fun add 0)
(fun 0 0)
(fun s 0)
(fun map 0)
(fun nil 0)
(fun cons 0)
(rule (app id x) x)
(rule (app add 0) id)
(rule (app (app add (app s x)) y) (app s (app (app add x) y)))
(rule (app (app map f) nil) nil)
(rule (app (app map f) (app (app cons x) xs)) (app (app cons (app f x)) (app (app map f) xs)))
