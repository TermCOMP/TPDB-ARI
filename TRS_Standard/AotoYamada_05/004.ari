; @xtcfilename "./TRS_Standard/AotoYamada_05/004.xml"
(format TRS)
(fun app 2)
(fun plus 0)
(fun |0| 0)
(fun s 0)
(fun sumwith 0)
(fun nil 0)
(fun cons 0)
(rule (app (app plus |0|) y) y)
(rule (app (app plus (app s x)) y) (app s (app (app plus x) y)))
(rule (app (app sumwith f) nil) nil)
(rule (app (app sumwith f) (app (app cons x) xs)) (app (app plus (app f x)) (app (app sumwith f) xs)))
