; @origtpdbfilename ./TRS/HM/t008.trs
; @xtcfilename "./TRS_Standard/HirokawaMiddeldorp_04/t008.xml"
(format TRS)
(fun half 1)
(fun |0| 0)
(fun s 1)
(fun log 1)
(rule (half |0|) |0|)
(rule (half (s |0|)) |0|)
(rule (half (s (s x))) (s (half x)))
(rule (s (log |0|)) (s |0|))
(rule (log (s x)) (s (log (half (s x)))))
