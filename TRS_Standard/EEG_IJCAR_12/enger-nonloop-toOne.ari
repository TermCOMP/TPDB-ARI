; @origtpdbfilename enger-nonloop-toOne.trs
; @xtcfilename "./TRS_Standard/EEG_IJCAR_12/enger-nonloop-toOne.xml"
(format TRS)
(fun f 2)
(fun tt 0)
(fun eq 2)
(fun toOne 1)
(fun s 1)
(fun |0| 0)
(rule (f tt x) (f (eq (toOne x) (s |0|)) (s x)))
(rule (eq (s x) (s y)) (eq x y))
(rule (eq |0| |0|) tt)
(rule (toOne (s (s x))) (toOne (s x)))
(rule (toOne (s |0|)) (s |0|))
