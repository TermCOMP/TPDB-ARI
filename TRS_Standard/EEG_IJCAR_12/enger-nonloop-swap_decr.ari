; @origtpdbfilename enger-nonloop-swap_decr.trs
; @xtcfilename "./TRS_Standard/EEG_IJCAR_12/enger-nonloop-swap_decr.xml"
(format TRS)
(fun f 2)
(fun tt 0)
(fun swap 2)
(fun |0| 0)
(fun s 1)
(fun decr 1)
(rule (f tt x) (f (swap x |0|) (s x)))
(rule (swap (s x) y) (swap x (s y)))
(rule (swap |0| (s y)) (decr (s y)))
(rule (decr (s y)) (decr y))
(rule (decr |0|) tt)
