; @origtpdbfilename emmes-nonloop-ex2_2.trs
; @xtcfilename "./TRS_Standard/EEG_IJCAR_12/emmes-nonloop-ex2_2.xml"
(format TRS)
(fun f 3)
(fun true 0)
(fun gt 2)
(fun double 1)
(fun s 1)
(fun |0| 0)
(fun false 0)
(rule (f true x y) (f (gt x y) (double x) (s y)))
(rule (gt (s x) |0|) true)
(rule (gt |0| y) false)
(rule (gt (s x) (s y)) (gt x y))
(rule (double |0|) |0|)
(rule (double (s x)) (s (s (double x))))
