(format TRS)
(fun f 2)
(fun tt 0)
(fun isList 1)
(fun Cons 2)
(fun nil 0)
(rule (f tt x) (f (isList x) (Cons tt x)))
(rule (isList (Cons tt xs)) (isList xs))
(rule (isList nil) tt)
