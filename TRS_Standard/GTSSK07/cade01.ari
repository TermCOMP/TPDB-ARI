; @origtpdbfilename ./TRS/SchneiderKamp/trs/cade01.trs
; @xtcfilename "./TRS_Standard/GTSSK07/cade01.xml"
(format TRS)
(fun minus 2)
(fun cond 3)
(fun gt 2)
(fun false 0)
(fun 0 0)
(fun true 0)
(fun s 1)
(rule (minus x y) (cond (gt x y) x y))
(rule (cond false x y) 0)
(rule (cond true x y) (s (minus x (s y))))
(rule (gt 0 v) false)
(rule (gt (s u) 0) true)
(rule (gt (s u) (s v)) (gt u v))
