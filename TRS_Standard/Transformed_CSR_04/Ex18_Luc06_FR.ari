; @xtcfilename "./TRS_Standard/Transformed_CSR_04/Ex18_Luc06_FR.xml"
(format TRS)
(fun f 1)
(fun a 0)
(fun g 1)
(fun n__f 1)
(fun n__a 0)
(fun activate 1)
(rule (f (f a)) (f (g (n__f n__a))))
(rule (f X) (n__f X))
(rule a n__a)
(rule (activate (n__f X)) (f (activate X)))
(rule (activate n__a) a)
(rule (activate X) X)
