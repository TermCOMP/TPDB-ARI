(format TRS)
(fun f 1)
(fun a 0)
(fun c 1)
(fun n__f 1)
(fun n__g 1)
(fun n__a 0)
(fun g 1)
(fun activate 1)
(rule (f (f a)) (c (n__f (n__g (n__f n__a)))))
(rule (f X) (n__f X))
(rule (g X) (n__g X))
(rule a n__a)
(rule (activate (n__f X)) (f (activate X)))
(rule (activate (n__g X)) (g (activate X)))
(rule (activate n__a) a)
(rule (activate X) X)
