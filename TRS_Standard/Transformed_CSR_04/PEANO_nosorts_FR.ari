(format TRS)
(fun and 2)
(fun tt 0)
(fun activate 1)
(fun plus 2)
(fun |0| 0)
(fun s 1)
(rule (and tt X) (activate X))
(rule (plus N |0|) N)
(rule (plus N (s M)) (s (plus N M)))
(rule (activate X) X)
