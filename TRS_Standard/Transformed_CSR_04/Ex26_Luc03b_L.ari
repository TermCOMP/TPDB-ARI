; @origtpdbfilename ./TRS/TRCSR/Ex26_Luc03b_L.trs
; @xtcfilename "./TRS_Standard/Transformed_CSR_04/Ex26_Luc03b_L.xml"
(format TRS)
(fun terms 1)
(fun cons 1)
(fun recip 1)
(fun sqr 1)
(fun |0| 0)
(fun s 0)
(fun dbl 1)
(fun add 2)
(fun first 2)
(fun nil 0)
(rule (terms N) (cons (recip (sqr N))))
(rule (sqr |0|) |0|)
(rule (sqr s) s)
(rule (dbl |0|) |0|)
(rule (dbl s) s)
(rule (add |0| X) X)
(rule (add s Y) s)
(rule (first |0| X) nil)
(rule (first s (cons Y)) (cons Y))
