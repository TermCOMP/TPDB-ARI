; @origtpdbfilename ./TRS/TRCSR/Ex5_Zan97_FR.trs
; @xtcfilename "./TRS_Standard/Transformed_CSR_04/Ex5_Zan97_FR.xml"
(format TRS)
(fun f 1)
(fun if 3)
(fun c 0)
(fun n__f 1)
(fun n__true 0)
(fun true 0)
(fun false 0)
(fun activate 1)
(rule (f X) (if X c (n__f n__true)))
(rule (if true X Y) X)
(rule (if false X Y) (activate Y))
(rule (f X) (n__f X))
(rule true n__true)
(rule (activate (n__f X)) (f (activate X)))
(rule (activate n__true) true)
(rule (activate X) X)
