; @origtpdbfilename ./TRS/Thiemann/sizeChange.trs
; @xtcfilename "./TRS_Standard/AProVE_06/sizeChange.xml"
(format TRS)
(fun r 4)
(fun nil 0)
(fun cons 2)
(fun succ 1)
(fun zero 0)
(rule (r xs ys zs nil) xs)
(rule (r xs nil zs (cons w ws)) (r xs xs (cons (succ zero) zs) ws))
(rule (r xs (cons y ys) nil (cons w ws)) (r xs xs (cons (succ zero) nil) ws))
(rule (r xs (cons y ys) (cons z zs) (cons w ws)) (r ys (cons y ys) zs (cons (succ zero) (cons w ws))))
