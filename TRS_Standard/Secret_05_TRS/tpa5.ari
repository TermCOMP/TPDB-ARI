; @origtpdbfilename ./TRS/secret05/tpa5.trs
; @xtcfilename "./TRS_Standard/Secret_05_TRS/tpa5.xml"
(format TRS)
(fun - 2)
(fun 0 0)
(fun s 1)
(fun min 2)
(fun twice 1)
(fun f 2)
(rule (- x 0) x)
(rule (- (s x) (s y)) (- x y))
(rule (min x 0) 0)
(rule (min 0 y) 0)
(rule (min (s x) (s y)) (s (min x y)))
(rule (twice 0) 0)
(rule (twice (s x)) (s (s (twice x))))
(rule (f (s x) (s y)) (f (- y (min x y)) (s (twice (min x y)))))
(rule (f (s x) (s y)) (f (- x (min x y)) (s (twice (min x y)))))
