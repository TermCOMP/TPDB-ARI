(format TRS)
(fun f 2)
(fun cond 3)
(fun and 2)
(fun isNat 1)
(fun tt 0)
(fun s 1)
(fun 0 0)
(fun ff 0)
(rule (f x y) (cond (and (isNat x) (isNat y)) x y))
(rule (cond tt x y) (f (s x) (s y)))
(rule (isNat (s x)) (isNat x))
(rule (isNat 0) tt)
(rule (and tt tt) tt)
(rule (and ff x) ff)
(rule (and x ff) ff)
