(format LCTRS)
(theory Ints)
(fun l0 (-> Int Int))
(fun l1 (-> Int Int))
(fun l2 (-> Int Int))
(fun l3 (-> Int Int))
(entrypoint l3)
(rule (l0 p^0) (l1 p^post) :guard (= p^0 p^post))
(rule (l1 p^0) (l0 p^post) :guard (= p^0 p^post))
(rule
(l2 p^0)
(l0 p^post)
:guard
(exists
((p^1 Int) (p^2 Int))
(and (and (= p^1 0) (= p^2 1)) (= p^post 0)))
)

(rule (l3 p^0) (l2 p^post) :guard (= p^0 p^post))
