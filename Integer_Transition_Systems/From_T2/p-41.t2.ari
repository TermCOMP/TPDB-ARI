(format LCTRS)
(theory Ints)
(fun l0 (-> Int Int))
(fun l1 (-> Int Int))
(fun l2 (-> Int Int))
(entrypoint l2)
(rule
(l0 Result_4^0)
(l1 Result_4^post)
:guard
(exists
((Result_4^1 Int))
(and (= Result_4^1 Result_4^1) (= Result_4^post Result_4^post)))
)

(rule
