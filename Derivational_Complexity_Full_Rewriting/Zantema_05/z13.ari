; @origtpdbfilename ./TRS/Zantema/z13.trs
; @xtcfilename "./Derivational_Complexity_Full_Rewriting/Zantema_05/z13.xml"
(format TRS)
(fun f 2)
(fun a 1)
(fun b 1)
(rule (f x (a (b y))) (f (a (a x)) y))
(rule (f x (b (a y))) (f (b (b x)) y))
(rule (f (a x) y) (f x (a y)))
(rule (f (b x) y) (f x (b y)))
