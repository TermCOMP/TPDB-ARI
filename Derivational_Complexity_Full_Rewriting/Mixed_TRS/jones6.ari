; @origtpdbfilename ./TRS/LJB01/jones6.trs
; @xtcfilename "./Derivational_Complexity_Full_Rewriting/Mixed_TRS/jones6.xml"
(format TRS)
(fun f 2)
(fun empty 0)
(fun g 2)
(fun cons 2)
(rule (f a empty) (g a empty))
(rule (f a (cons x k)) (f (cons x a) k))
(rule (g empty d) d)
(rule (g (cons x k) d) (g k (cons x d)))
