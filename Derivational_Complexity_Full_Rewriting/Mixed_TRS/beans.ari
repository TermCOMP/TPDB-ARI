; @origtpdbfilename ./TRS/Zantema06/beans.trs
; @xtcfilename "./Derivational_Complexity_Full_Rewriting/Mixed_TRS/beans.xml"
(format TRS)
(fun f 2)
(fun s 1)
(fun L 1)
(fun |0| 0)
(fun nil 0)
(rule (f x (f (s (s y)) (f z w))) (f (s x) (f y (f (s z) w))))
(rule (L (f (s (s y)) (f z w))) (L (f (s |0|) (f y (f (s z) w)))))
(rule (f x (f (s (s y)) nil)) (f (s x) (f y (f (s |0|) nil))))
