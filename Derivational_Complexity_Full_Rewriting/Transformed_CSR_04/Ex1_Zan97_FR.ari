; @origtpdbfilename ./TRS/TRCSR/Ex1_Zan97_FR.trs
; @origtpdbfilename ./TRS/TRCSR/Ex1_Zan97_Z.trs
; @xtcfilename "./Derivational_Complexity_Full_Rewriting/Transformed_CSR_04/Ex1_Zan97_FR.xml"
(format TRS)
(fun g 1)
(fun h 1)
(fun activate 1)
(fun c 0)
(fun d 0)
(fun n__d 0)
(fun n__c 0)
(rule (g X) (h (activate X)))
(rule c d)
(rule (h n__d) (g n__c))
(rule d n__d)
(rule c n__c)
(rule (activate n__d) d)
(rule (activate n__c) c)
(rule (activate X) X)
