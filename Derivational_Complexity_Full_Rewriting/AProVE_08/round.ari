(format TRS)
(fun f 2)
(fun s 1)
(fun round 1)
(fun |0| 0)
(rule (f (s x) x) (f (s x) (round (s x))))
(rule (round |0|) |0|)
(rule (round |0|) (s |0|))
(rule (round (s |0|)) (s |0|))
(rule (round (s (s x))) (s (s (round x))))
