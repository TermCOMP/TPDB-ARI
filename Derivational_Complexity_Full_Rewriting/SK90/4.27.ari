(format TRS)
(fun int 2)
(fun 0 0)
(fun . 2)
(fun nil 0)
(fun s 1)
(fun int_list 1)
(rule (int 0 0) (. 0 nil))
(rule (int 0 (s y)) (. 0 (int (s 0) (s y))))
(rule (int (s x) 0) nil)
(rule (int (s x) (s y)) (int_list (int x y)))
(rule (int_list nil) nil)
(rule (int_list (. x y)) (. (s x) (int_list y)))
