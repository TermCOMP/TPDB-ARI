; @origtpdbfilename ./TRS/SK90/2.39.trs
; @xtcfilename "./Derivational_Complexity_Full_Rewriting/SK90/2.39.xml"
(format TRS)
(fun rev 1)
(fun nil 0)
(fun . 2)
(fun ++ 2)
(fun car 1)
(fun cdr 1)
(fun null 1)
(fun true 0)
(fun false 0)
(rule (rev nil) nil)
(rule (rev (. x y)) (++ (rev y) (. x nil)))
(rule (car (. x y)) x)
(rule (cdr (. x y)) y)
(rule (null nil) true)
(rule (null (. x y)) false)
(rule (++ nil y) y)
(rule (++ (. x y) z) (. x (++ y z)))
