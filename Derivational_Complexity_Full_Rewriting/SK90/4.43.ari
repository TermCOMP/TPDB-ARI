; @origtpdbfilename ./TRS/SK90/4.43.trs
; @xtcfilename "./Derivational_Complexity_Full_Rewriting/SK90/4.43.xml"
(format TRS)
(fun + 2)
(fun 0 0)
(fun s 1)
(fun f 1)
(fun g 1)
(fun h 2)
(rule (+ x 0) x)
(rule (+ x (s y)) (s (+ x y)))
(rule (+ 0 y) y)
(rule (+ (s x) y) (s (+ x y)))
(rule (+ x (+ y z)) (+ (+ x y) z))
(rule (f (g (f x))) (f (h (s 0) x)))
(rule (f (g (h x y))) (f (h (s x) y)))
(rule (f (h x (h y z))) (f (h (+ x y) z)))
