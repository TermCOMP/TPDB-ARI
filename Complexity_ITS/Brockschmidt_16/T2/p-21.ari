(format LCTRS)
(theory Ints)
(fun f1 (-> Int Int Int Int))
(fun f2 (-> Int Int Int Int))
(fun f3 (-> Int Int Int Int))
(fun f300 (-> Int Int Int Int))
(entrypoint f3)
(rule (f2 A B C) (f2 (+ (- 1) A) B C) :guard (>= A 31))
