(format LCTRS)
(theory Ints)
(fun f1 (-> Int Int Int))
(fun f2 (-> Int Int Int))
(fun f999 (-> Int Int Int))
(entrypoint f999)
(rule (f1 A B) (f2 (- A 1) B) :guard (>= A 1))
(rule (f2 A B) (f2 (- A 1) (+ B 1)) :guard (>= A 1))
(rule (f999 A B) (f1 1 (- B 1)) :guard (and (>= B 1) (= A 0)))
(rule (f1 A B) (f1 (+ A 1) (- B 1)) :guard (>= B 1))
(rule (f2 A B) (f1 (+ A 1) (- B 1)) :guard (>= B 1))
