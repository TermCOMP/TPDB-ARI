(format LCTRS)
(theory Ints)
(fun f1 (-> Int Int Int Int Int))
(fun f2 (-> Int Int Int Int Int))
(fun f3 (-> Int Int Int Int Int))
(entrypoint f3)
(rule (f3 A B C D) (f1 0 B C D))
(rule (f1 A B C D) (f2 A B C E) :guard (>= B C))
(rule
  (f1 A B C D)
  (f2 1 (+ 1 B) C E)
  :guard
  (and (>= 0 A) (= (+ B 1) C)))

(rule
  (f1 A B C D)
  (f1 0 (+ 1 B) (+ (- 1) C) D)
  :guard
  (and (and (>= C (+ 1 B)) (>= 0 A)) (>= C (+ 2 B))))

