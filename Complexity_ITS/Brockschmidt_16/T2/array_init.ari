(format LCTRS)
(theory Ints)
(fun f0 (-> Int Int))
(fun f11 (-> Int Int))
(fun f3 (-> Int Int))
(entrypoint f0)
(rule (f0 A) (f3 0))
(rule (f3 A) (f3 (+ A 1)) :guard (>= 9 A))
(rule
  (f3 A)
  (f11 A)
  :guard
  (exists ((B Int)) (and (>= A 10) (>= 0 (+ B 1)))))

(rule (f3 A) (f11 A) :guard (>= A 10))
