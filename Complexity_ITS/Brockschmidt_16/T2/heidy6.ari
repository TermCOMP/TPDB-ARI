(format LCTRS)
(theory Ints)
(fun f0 (-> Int Int Int Int))
(fun f2 (-> Int Int Int Int))
(fun f3 (-> Int Int Int Int))
(fun f5 (-> Int Int Int Int))
(entrypoint f0)
(rule (f2 A B C) (f2 (+ A 1) B C))
(rule (f3 A B C) (f3 A (- B 1) C) :guard (>= B 1))
(rule (f5 A B C) (f5 A B 1))
(rule (f0 A B C) (f2 0 B C) :guard (>= C 1))
(rule (f0 A B C) (f3 0 B C) :guard (>= 0 C))
(rule (f3 A B C) (f5 0 B C) :guard (>= 0 B))
