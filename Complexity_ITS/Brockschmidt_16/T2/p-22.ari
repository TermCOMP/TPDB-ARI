(format LCTRS)
(theory Ints)
(fun f1 (-> Int Int Int))
(fun f300 (-> Int Int Int))
(fun f5 (-> Int Int Int))
(entrypoint f300)
(rule (f5 A B) (f5 (+ (- 1) A) B) :guard (>= A 1))
(rule (f5 A B) (f1 A C) :guard (>= 0 A))
(rule (f300 A B) (f5 (+ (- 1) A) B) :guard (>= A 1))
(rule (f300 A B) (f1 A C) :guard (>= 0 A))
