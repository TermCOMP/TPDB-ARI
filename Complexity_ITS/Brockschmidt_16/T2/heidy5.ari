(format LCTRS)
(theory Ints)
(fun f0 (-> Int Int Int Int))
(fun f2 (-> Int Int Int Int))
(fun f4 (-> Int Int Int Int))
(fun f5 (-> Int Int Int Int))
(entrypoint f0)
(rule (f4 A B C) (f5 A B 1) :guard (and (>= 0 A) (>= 0 B)))
(rule (f0 A B C) (f2 A B 1) :guard (>= A 1))
(rule (f4 A B C) (f4 A (- B 1) C) :guard (>= B 1))
(rule (f0 A B C) (f4 A B 0) :guard (>= 0 A))
