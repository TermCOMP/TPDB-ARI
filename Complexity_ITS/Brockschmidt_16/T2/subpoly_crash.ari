(format LCTRS)
(theory Ints)
(fun f0 (-> Int Int Int))
(fun f1 (-> Int Int Int))
(fun f10000 (-> Int Int Int))
(fun f2 (-> Int Int Int))
(entrypoint f0)
(rule (f0 A B) (f1 A A) :guard (= A B))
(rule (f1 A B) (f1 (+ A 1) (+ B 1)))
(rule (f1 A B) (f2 A B))
(rule (f1 A B) (f10000 A B) :guard (>= A (+ B 1)))
