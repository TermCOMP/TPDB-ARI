(format LCTRS)
(theory Ints)
(fun f1 (-> Int Int Int Int))
(fun f2 (-> Int Int Int Int))
(fun f3 (-> Int Int Int Int))
(entrypoint f2)
(rule (f1 A B C) (f3 A D C) :guard (>= A 500))
(rule (f1 A B C) (f1 (+ 1 A) B C) :guard (>= 499 A))
(rule (f2 A B C) (f3 E D E) :guard (>= E 500))
(rule (f2 A B C) (f1 (+ 1 D) B D) :guard (>= 499 D))
