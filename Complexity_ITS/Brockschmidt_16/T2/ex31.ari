(format LCTRS)
(theory Ints)
(fun f0 (-> Int Int Int))
(fun f11 (-> Int Int Int))
(fun f4 (-> Int Int Int))
(entrypoint f0)
(rule (f0 A B) (f4 0 99))
(rule
  (f4 A B)
  (f4 E B)
  :guard
  (exists ((C Int) (D Int)) (and (>= B (+ A 1)) (>= C (+ D 1)))))

(rule (f4 A B) (f4 A E) :guard (>= B (+ A 1)))
(rule (f4 A B) (f11 A B) :guard (>= A B))
