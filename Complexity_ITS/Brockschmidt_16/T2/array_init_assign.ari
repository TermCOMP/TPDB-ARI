(format LCTRS)
(theory Ints)
(fun f0 (-> Int Int Int))
(fun f10 (-> Int Int Int))
(fun f18 (-> Int Int Int))
(fun f4 (-> Int Int Int))
(entrypoint f0)
(rule (f0 A B) (f4 0 B))
(rule (f4 A B) (f4 (+ A 1) B) :guard (>= 1 A))
(rule (f10 A B) (f10 A (+ B 1)) :guard (>= 1 B))
(rule
  (f10 A B)
  (f18 A B)
  :guard
  (exists ((C Int)) (and (>= B 2) (>= 0 (+ C 1)))))

(rule (f10 A B) (f18 A B) :guard (>= B 2))
(rule (f4 A B) (f10 A 0) :guard (>= A 2))
