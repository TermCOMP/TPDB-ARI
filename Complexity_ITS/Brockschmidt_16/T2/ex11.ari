(format LCTRS)
(theory Ints)
(fun f0 (-> Int Int Int))
(fun f12 (-> Int Int Int))
(fun f4 (-> Int Int Int))
(fun f5 (-> Int Int Int))
(entrypoint f0)
(rule (f4 A B) (f5 A B) :guard (>= 0 (+ A 1)))
(rule (f4 A B) (f5 A B) :guard (>= A 1))
(rule (f0 A B) (f4 C 0))
(rule (f5 A B) (f4 C (+ B 1)) :guard (>= 3 B))
(rule (f5 A B) (f4 C (+ B 1)) :guard (>= B 5))
(rule (f5 A B) (f4 C 1) :guard (= B 4))
(rule (f4 A B) (f12 0 B) :guard (= A 0))
