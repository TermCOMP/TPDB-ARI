(format LCTRS)
(theory Ints)
(fun f0 (-> Int Int Int Int Int))
(fun f23 (-> Int Int Int Int Int))
(fun f8 (-> Int Int Int Int Int))
(entrypoint f0)
(rule (f0 A B C D) (f8 0 B C D))
(rule (f8 A B C D) (f8 (+ A 1) B C D) :guard (>= 3 A))
(rule (f8 A B C D) (f8 (+ A 1) A (+ A 1) E) :guard (>= 3 A))
(rule
  (f8 A B C D)
  (f23 A B C D)
  :guard
  (exists ((E Int)) (and (>= A 4) (>= 0 (+ E 1)))))
(rule (f8 A B C D) (f23 A B C D) :guard (>= A 4))
