(format LCTRS)
(theory Ints)
(fun f2 (-> Int Int Int Int))
(fun f3 (-> Int Int Int Int))
(fun f4 (-> Int Int Int Int))
(entrypoint f3)
(rule (f2 A B C) (f2 (+ (- B) A) (+ 1 B) C) :guard (>= A 1))
(rule (f3 A B C) (f2 A B C) :guard (>= B 1))
(rule (f2 A B C) (f4 A B D) :guard (>= 0 A))
(rule (f3 A B C) (f4 A B D) :guard (>= 0 B))
