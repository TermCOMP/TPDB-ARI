(format LCTRS)
(theory Ints)
(fun f4 (-> Int Int Int Int Int Int Int Int))
(fun f5 (-> Int Int Int Int Int Int Int Int))
(fun f6 (-> Int Int Int Int Int Int Int Int))
(entrypoint f5)
(rule (f4 A B C D E F G) (f4 (+ A B) B C D E F G) :guard (>= A 0))
(rule (f4 A B C D E F G) (f6 A B 0 0 0 0 0) :guard (>= 0 (+ A 1)))
(rule (f5 A B C D E F G) (f4 A B C D E F G) :guard (>= 0 (+ B 1)))
(rule (f5 A B C D E F G) (f6 A B 0 0 0 0 0) :guard (>= B 0))
