(format LCTRS)
(theory Ints)
(fun f0 (-> Int Int))
(fun f11 (-> Int Int))
(fun f14 (-> Int Int))
(fun f4 (-> Int Int))
(fun f5 (-> Int Int))
(entrypoint f0)
(rule (f4 A) (f5 A) :guard (>= 0 (+ B 1)))
(rule (f4 A) (f5 A))
(rule (f0 A) (f4 0))
(rule (f5 A) (f11 A) :guard (>= A 3))
(rule (f4 A) (f11 A))
(rule (f5 A) (f4 (+ A 1)) :guard (>= 2 A))
(rule (f11 A) (f14 A) :guard (>= 1 A))
(rule (f11 A) (f14 A) :guard (>= A 2))
