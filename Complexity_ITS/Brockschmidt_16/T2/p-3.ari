(format LCTRS)
(theory Ints)
(fun f1 (-> Int Int Int Int))
(fun f2 (-> Int Int Int Int))
(fun f300 (-> Int Int Int Int))
(entrypoint f2)
(rule (f2 A B C) (f1 A B C))
(rule (f1 A B C) (f1 (+ 1 A) B C) :guard (>= B (+ 1 A)))
(rule
  (f1 A B C)
  (f1 (+ 1 A) A C)
  :guard
  (exists ((D Int)) (and (>= B D) (= A B))))

(rule (f1 A B C) (f300 A B D) :guard (and (>= A B) (>= A (+ B 1))))
(rule (f1 A B C) (f300 A B D) :guard (and (>= A B) (>= B (+ A 1))))
