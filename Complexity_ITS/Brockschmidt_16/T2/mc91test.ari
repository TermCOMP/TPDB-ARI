(format LCTRS)
(theory Ints)
(fun f0 (-> Int Int Int Int Int Int))
(fun f1 (-> Int Int Int Int Int Int))
(fun f2 (-> Int Int Int Int Int Int))
(entrypoint f0)
(rule (f0 A B C D E) (f1 F 1 0 D E))
(rule
  (f1 A B C D E)
  (f1 (- A 10) (- B 1) C D E)
  :guard
  (and (>= B 1) (>= A 101)))

(rule
  (f1 A B C D E)
  (f1 (+ A 11) (+ B 1) C D E)
  :guard
  (and (>= B 1) (>= 100 A)))

(rule
  (f1 A B C D E)
  (f1 (- A 10) (- B 1) 1 A B)
  :guard
  (and (and (>= A 101) (>= 0 C)) (>= B 1)))

(rule
  (f1 A B C D E)
  (f1 (+ A 11) (+ B 1) 1 A B)
  :guard
  (and (and (>= 100 A) (>= 0 C)) (>= B 1)))

(rule
  (f1 A B C D E)
  (f2 A B C D E)
  :guard
  (and (and (>= D A) (>= C 1)) (>= B E)))

