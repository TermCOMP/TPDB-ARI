(format LCTRS)
(theory Ints)
(fun eval1 (-> Int Int Int))
(fun eval2 (-> Int Int Int))
(fun start (-> Int Int Int))
(entrypoint start)
(rule (eval1 A B) (eval2 A B) :guard (>= A 1))
(rule (eval2 A B) (eval2 A (- B 1)) :guard (and (>= A 1) (>= B 1)))
(rule (eval2 A B) (eval1 (- A 1) B) :guard (and (>= A 1) (>= 0 B)))
(rule (start A B) (eval1 A B))
