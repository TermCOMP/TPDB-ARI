(format LCTRS)
(theory Ints)
(fun eval1 (-> Int Int Int))
(fun eval2 (-> Int Int Int))
(fun start (-> Int Int Int))
(entrypoint start)
(rule (eval1 A B) (eval2 A 0) :guard (>= A 0))
(rule (eval2 A B) (eval2 A (+ B 1)) :guard (>= A (+ 1 B)))
(rule (eval2 A B) (eval1 (- A 1) B) :guard (>= B A))
(rule (start A B) (eval1 A B))
