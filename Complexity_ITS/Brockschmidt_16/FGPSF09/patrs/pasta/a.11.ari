(format LCTRS)
(theory Ints)
(fun eval1 (-> Int Int Int Int))
(fun eval2 (-> Int Int Int Int))
(fun start (-> Int Int Int Int))
(entrypoint start)
(rule (eval1 A B C) (eval2 A B C) :guard (>= A (+ B 1)))
(rule (eval2 A B C) (eval1 A (+ B 1) C) :guard (>= A (+ C 1)))
(rule (eval2 A B C) (eval1 A B (+ C 1)) :guard (>= A (+ C 1)))
(rule (eval2 A B C) (eval1 (- A 1) B C) :guard (>= C A))
(rule (start A B C) (eval1 A B C))
