(format LCTRS)
(theory Ints)
(fun div (-> Int Int Int))
(fun end (-> Int Int Int))
(fun start (-> Int Int Int))
(entrypoint start)
(rule (div A B) (end A B) :guard (>= 0 A))
(rule (div A B) (end A B) :guard (>= A B))
(rule (div A B) (div A (- B A)) :guard (and (>= B (+ A 1)) (>= A 1)))
(rule (start A B) (div A B))
