(format LCTRS)
(theory Ints)
(fun l0 (-> Int Int Int Int Int))
(fun l1 (-> Int Int Int Int Int))
(fun l2 (-> Int Int Int Int Int))
(entrypoint l0)
(rule (l0 A B C D) (l1 0 B C D))
(rule (l1 A B C D) (l2 A B 0 0) :guard (>= B 1))
(rule (l2 A B C D) (l2 A B (+ C 1) (+ D C)) :guard (>= B (+ C 1)))
(rule (l2 A B C D) (l1 (+ A D) (- B 1) C D) :guard (>= C B))
