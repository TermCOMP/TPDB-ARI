(format LCTRS)
(theory Ints)
(fun l0 (-> Int Int Int Int Int))
(fun l1 (-> Int Int Int Int Int))
(fun l2 (-> Int Int Int Int Int))
(fun l3 (-> Int Int Int Int Int))
(entrypoint l0)
(rule (l0 A B C D) (l1 0 B C D))
(rule (l1 A B C D) (l2 A B 0 0) :guard (> B 0))
(rule (l2 A B C D) (l2 A B (+ C 1) (+ D C)) :guard (< C B))
(rule (l2 A B C D) (l1 (+ A D) (- B 1) C D) :guard (>= C B))
(rule (l1 A B C D) (l3 A B C D) :guard (<= B 0))
(rule (l3 A B C D) (l3 (- A 1) B C D) :guard (> A 0))
