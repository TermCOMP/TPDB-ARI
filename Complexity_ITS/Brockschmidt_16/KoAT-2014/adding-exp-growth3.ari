(format LCTRS)
(theory Ints)
(fun f (-> Int Int Int Int))
(fun g (-> Int Int Int Int))
(fun g1 (-> Int Int Int Int))
(fun h (-> Int Int Int Int))
(entrypoint f)
(rule (f A B C) (g A 1 0))
(rule (g A B C) (g1 (- A 1) B B) :guard (> A 0))
(rule (g1 A B C) (g A (+ C B) C))
(rule (g A B C) (h A B C) :guard (<= A 0))
(rule (h A B C) (h A (- B 1) C) :guard (> B 0))
