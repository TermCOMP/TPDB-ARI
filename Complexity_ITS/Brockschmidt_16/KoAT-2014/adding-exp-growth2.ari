(format LCTRS)
(theory Ints)
(fun f (-> Int Int Int Int))
(fun g (-> Int Int Int Int))
(fun h (-> Int Int Int Int))
(entrypoint f)
(rule (f A B C) (g 1 1 C))
(rule (g A B C) (g (+ A B) (+ A B) (- C 1)) :guard (> C 0))
(rule (g A B C) (h A B C) :guard (<= C 0))
(rule (h A B C) (h A (- B 1) C) :guard (> B 0))
