(format LCTRS)
(theory Ints)
(fun a (-> Int Int))
(fun start (-> Int Int))
(entrypoint start)
(rule (start A) (a A) :guard (>= A 1))
(rule (start A) (a A) :guard (>= A 2))
(rule (start A) (a A) :guard (>= A 4))
(rule (a A) (a B) :guard (and (>= (* 2 B) 2) (= A (* 2 B))))
(rule (a A) (a B) :guard (and (>= (* 2 B) 1) (= A (+ (* 2 B) 1))))
