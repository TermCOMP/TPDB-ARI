(format LCTRS)
(theory Ints)
(fun a (-> Int Int))
(fun start (-> Int Int))
(entrypoint start)
(rule (start A) (a A) :guard (>= A 1))
(rule (start A) (a A) :guard (>= A 2))
(rule (start A) (a A) :guard (>= A 4))
(rule
  (a A)
  (a (* A B))
  :guard
  (and (and (>= 1 (* 2 B)) (>= (* 3 B) 2)) (>= A 2)))

