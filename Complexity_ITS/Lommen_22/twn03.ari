(format LCTRS)
(theory Ints)
(fun l0 (-> Int Int Int Int))
(fun l1 (-> Int Int Int Int))
(fun l2 (-> Int Int Int Int))
(entrypoint l0)
(rule (l0 A B C) (l1 A B C) :guard (> A 0))
