(format LCTRS)
(theory Ints)
(fun l0 (-> Int Int Int Int Int))
(fun l1 (-> Int Int Int Int Int))
(fun l2 (-> Int Int Int Int Int))
(fun l3 (-> Int Int Int Int Int))
(entrypoint l0)
(rule (l0 A B F X) (l1 A B F X))
(rule (l1 A B F X) (l1 A B (+ F X) (- X 1)) :guard (>= X 1))
(rule (l1 A B F X) (l2 A B F X) :guard (< X 1))
(rule (l2 A B F X) (l3 F F F X) :guard (>= F 1))
(rule (l3 A B F X) (l3 (+ A B) (- B 1) F X) :guard (>= A 1))
(rule (l3 A B F X) (l2 A B (- F 1) X) :guard (>= 0 A))
