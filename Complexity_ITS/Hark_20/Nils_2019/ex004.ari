(format LCTRS)
(theory Ints)
(fun l0 (-> Int Int Int Int Int Int))
(fun l1 (-> Int Int Int Int Int Int))
(fun l2 (-> Int Int Int Int Int Int))
(entrypoint l0)
(rule (l0 A B C D E) (l1 A B C D E))
(rule (l1 A B C D E) (l1 (+ A B) (+ B C) (- C 1) D E) :guard (>= A 1))
(rule (l1 A B C D E) (l2 A B C D E) :guard (>= 0 A))
(rule (l2 A B C D E) (l2 A B C (+ D E) (- E 1)) :guard (>= D 1))
