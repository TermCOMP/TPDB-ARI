(format LCTRS)
(theory Ints)
(fun l0 (-> Int Int Int Int Int))
(fun l1 (-> Int Int Int Int Int))
(fun l2 (-> Int Int Int Int Int))
(entrypoint l0)
(rule (l0 A B C F) (l1 A B C F))
(rule (l1 A B C F) (l1 (+ A B) (+ B C) (- C 1) F) :guard (>= A 1))
(rule (l1 A B C F) (l2 A B C (- F 1)) :guard (>= 0 A))
(rule (l2 A B C F) (l1 F F F F) :guard (>= F 1))
