(format LCTRS)
(theory Ints)
(fun l0 (-> Int Int Int Int Int))
(fun l1 (-> Int Int Int Int Int))
(fun l2 (-> Int Int Int Int Int))
(fun l3 (-> Int Int Int Int Int))
(entrypoint l0)
(rule (l0 A B C D) (l1 A B C D))
(rule (l1 A B C D) (l1 A B (+ C D) (- D 1)) :guard (> D 0))
(rule (l1 A B C D) (l2 A B C D) :guard (<= D 0))
(rule (l2 A B C D) (l3 C C C D) :guard (> C 0))
(rule (l3 A B C D) (l3 (+ A B) (- B 1) C D) :guard (> A 0))
(rule (l3 A B C D) (l2 A B (- C 1) D) :guard (<= A 0))
