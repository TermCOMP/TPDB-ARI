(format TRS)
(fun b0 1)
(fun $ 1)
(fun u 1)
(fun t0 1)
(fun & 1)
(rule (b0 ($ x1)) ($ x1))
(rule (b0 (u ($ x1))) (t0 (u (u ($ x1)))))
