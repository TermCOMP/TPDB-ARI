(format TRS)
(fun p 1)
(fun 0 1)
(fun s 1)
(fun f 1)
(fun g 1)
(fun q 1)
(fun i 1)
(rule (p (0 x1)) (s (s (0 (s (s (p x1)))))))
(rule (p (s (0 x1))) (0 x1))
(rule (p (s (s x1))) (s (p (s x1))))
(rule (f (s x1)) (g (q (i x1))))
(rule (g x1) (f (p (p x1))))
(rule (q (i x1)) (q (s x1)))
(rule (q (s x1)) (s (s x1)))
(rule (i x1) (s x1))
