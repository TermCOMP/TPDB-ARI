(format PTRS)
(fun rw 1)
(fun tree 2)
(fun tree_dup 1)
(fun tree_inc 1)
(fun pair 2)
(fun leaf 0)
(prule (rw (tree x y)) (((rw (tree_dup (tree x y))) :prob 1 ) ((rw x) :prob 1 )))
(prule (tree_inc (pair x y)) (((pair (tree x y) (tree x y)) :prob 1 )))
(prule (tree_dup (tree x y)) (((tree_inc (pair (tree_dup x) (tree_dup y))) :prob 1 )))
(prule (tree_dup leaf) (((pair leaf leaf) :prob 1 )))
