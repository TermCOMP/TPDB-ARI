(format PTRS)
(fun double 1)
(fun 0 0)
(fun loop2 1)
(fun s 1)
(fun stop 0)
(fun loop 1)
(prule (double 0) ((0 :prob 1 )))
(prule (loop2 (s x)) (((loop2 x) :prob 1 )))
(prule (loop2 0) ((stop :prob 1 )))
(prule (loop x) (((loop (double x)) :prob 1 ) ((loop2 x) :prob 1 )))
(prule (double (s x)) (((s (s (double x))) :prob 1 )))
