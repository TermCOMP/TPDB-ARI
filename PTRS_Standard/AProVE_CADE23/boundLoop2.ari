(format PTRS)
(fun outerLoop 2)
(fun s 1)
(fun minus 2)
(fun innerLoop 2)
(fun 0 0)
(fun stop 0)
(prule (outerLoop (s x) (s y)) (((outerLoop (minus x (innerLoop y 0)) (s y)) :prob 1 )))
(prule (outerLoop 0 (s y)) ((stop :prob 1 )))
(prule (minus x 0) ((x :prob 1 )))
(prule (innerLoop (s x) y) (((innerLoop x (s y)) :prob 1 ) ((innerLoop (s x) (s y)) :prob 1 )))
(prule (innerLoop 0 y) ((y :prob 1 )))
(prule (minus (s x) (s y)) (((minus x y) :prob 1 )))
