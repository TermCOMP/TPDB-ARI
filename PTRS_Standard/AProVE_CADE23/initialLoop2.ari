(format PTRS)
(fun loop2 1)
(fun s 1)
(fun ackermann 2)
(fun 0 0)
(fun loop 1)
(fun stop 0)
(prule (loop2 (s x)) (((loop2 x) :prob 1 )))
(prule (ackermann (s x) 0) (((ackermann x (s 0)) :prob 1 )))
(prule (loop x) (((loop (ackermann x x)) :prob 1 ) ((loop2 x) :prob 1 )))
(prule (loop2 0) ((stop :prob 1 )))
(prule (ackermann 0 x) (((s x) :prob 1 )))
(prule (ackermann (s x) (s y)) (((ackermann x (ackermann (s x) y)) :prob 1 )))
