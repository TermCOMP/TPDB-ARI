(format PTRS)
(fun app 2)
(fun cons 2)
(fun destroy 1)
(fun nil 0)
(prule (app (cons x xs) ys) (((cons x (app xs ys)) :prob 1 )))
(prule (destroy nil) ((nil :prob 1 )))
(prule (app nil ys) ((ys :prob 1 )))
(prule (destroy (cons x xs)) (((destroy xs) :prob 1 ) ((destroy (app xs (cons x (cons x nil)))) :prob 1 )))
