(format PTRS)
(fun leq 2)
(fun 0 0)
(fun true 0)
(fun loop 1)
(fun ifLoop 2)
(fun s 1)
(fun false 0)
(fun stop 0)
(prule (leq 0 x) ((true :prob 1 )))
(prule (loop x) (((ifLoop (leq x 0) x) :prob 1 )))
(prule (leq (s x) (s y)) (((leq x y) :prob 1 )))
(prule (leq (s x) 0) ((false :prob 1 )))
(prule (ifLoop false x) ((stop :prob 1 )))
(prule (ifLoop true x) (((loop (s x)) :prob 1 ) ((loop 0) :prob 1 )))
