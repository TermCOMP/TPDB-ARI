(format PTRS)
(fun minus 2)
(fun 0 0)
(fun div 2)
(fun s 1)
(prule (minus x x) ((0 :prob 1 )))
(prule (minus x 0) ((x :prob 1 )))
(prule (div (s x) (s y)) (((s (div (minus x y) (s y))) :prob 1 ) ((div (s x) (s y)) :prob 1 )))
(prule (minus (s x) (s y)) (((minus x y) :prob 1 )))
(prule (div 0 (s x)) ((0 :prob 1 )))
