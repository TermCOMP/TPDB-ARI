(format PTRS)
(fun loop 1)
(fun true 0)
(fun eq 2)
(fun coin 0)
(fun head 0)
(fun tail 0)
(fun false 0)
(prule (loop true) (((loop (eq coin coin)) :prob 1 )))
(prule coin ((head :prob 1 ) (tail :prob 1 )))
(prule (eq head head) ((true :prob 1 )))
(prule (eq tail head) ((false :prob 1 )))
(prule (eq head tail) ((false :prob 1 )))
(prule (eq tail tail) ((true :prob 1 )))
