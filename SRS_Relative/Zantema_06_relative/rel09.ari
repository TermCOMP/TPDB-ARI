(format TRS)
(fun b 1)
(fun q 1)
(fun |0| 1)
(fun |1| 1)
(fun p 1)
(rule (b (q (b x1))) (b (p (b x1))))
(rule (|0| (p (|0| x1))) (q x1) :cost 0)
(rule (|1| (p (|1| x1))) (q x1) :cost 0)
(rule (|0| (q (|0| x1))) (q x1) :cost 0)
(rule (|1| (q (|1| x1))) (q x1) :cost 0)
(rule (p x1) (|1| (p (|1| (|0| (|1| x1))))) :cost 0)
