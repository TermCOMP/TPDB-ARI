(format TRS)
(fun s 1)
(fun c 2)
(fun f 1)
(fun rand 1)
(fun g 1)
(rule (f (c (s x) y)) (f (c x (s y))))
(rule (f (c (s x) (s y))) (g (c x y)))
(rule (g (c x (s y))) (g (c (s x) y)))
(rule (g (c (s x) (s y))) (f (c x y)))
(rule (rand x) x :cost 0)
(rule (rand x) (rand (s x)) :cost 0)
