; @xtcfilename "./TRS_Relative/INVY_15/#3.16_rand.xml"
(format TRS)
(fun s 1)
(fun plus 2)
(fun |0| 0)
(fun times 2)
(fun rand 1)
(rule (times x |0|) |0|)
(rule (times x (s y)) (plus (times x y) x))
(rule (plus x |0|) x)
(rule (plus |0| x) x)
(rule (plus x (s y)) (s (plus x y)))
(rule (plus (s x) y) (s (plus x y)))
(rule (rand x) x :cost 0)
(rule (rand x) (rand (s x)) :cost 0)
