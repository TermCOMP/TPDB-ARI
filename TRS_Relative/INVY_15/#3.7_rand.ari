; @xtcfilename "./TRS_Relative/INVY_15/#3.7_rand.xml"
(format TRS)
(fun s 1)
(fun half 1)
(fun log 1)
(fun 0 0)
(fun rand 1)
(rule (half 0) 0)
(rule (half (s (s x))) (s (half x)))
(rule (log (s 0)) 0)
(rule (log (s (s x))) (s (log (s (half x)))))
(rule (rand x) x :cost 0)
(rule (rand x) (rand (s x)) :cost 0)
