; @xtcfilename "./TRS_Relative/INVY_15/#3.36_rand.xml"
(format TRS)
(fun s 1)
(fun f 1)
(fun |0| 0)
(fun minus 2)
(fun rand 1)
(fun g 1)
(rule (minus x |0|) x)
(rule (minus (s x) (s y)) (minus x y))
(rule (f |0|) (s |0|))
(rule (f (s x)) (minus (s x) (g (f x))))
(rule (g |0|) |0|)
(rule (g (s x)) (minus (s x) (f (g x))))
(rule (rand x) x :cost 0)
(rule (rand x) (rand (s x)) :cost 0)
