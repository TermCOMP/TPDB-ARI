(format TRS)
(fun s 1)
(fun true 0)
(fun c 1)
(fun f 1)
(fun if 3)
(fun |0| 0)
(fun |1| 0)
(fun rand 1)
(fun g 2)
(fun false 0)
(rule (f |0|) true)
(rule (f |1|) false)
(rule (f (s x)) (f x))
(rule (if true (s x) (s y)) (s x))
(rule (if false (s x) (s y)) (s y))
(rule (g x (c y)) (c (g x y)))
(rule (g x (c y)) (g x (if (f x) (c (g (s x) y)) (c y))))
(rule (rand x) x :cost 0)
(rule (rand x) (rand (s x)) :cost 0)
