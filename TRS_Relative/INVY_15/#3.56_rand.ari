(format TRS)
(fun s 1)
(fun c 2)
(fun d 1)
(fun f 1)
(fun rand 1)
(fun g 1)
(rule (g (c x (s y))) (g (c (s x) y)))
(rule (f (c (s x) y)) (f (c x (s y))))
