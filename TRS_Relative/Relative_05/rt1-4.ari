; @xtcfilename "./TRS_Relative/Relative_05/rt1-4.xml"
(format TRS)
(fun r 1)
(fun f 2)
(fun l 1)
(fun el 1)
(rule (f (el x) y) (f x (el y)))
(rule (f x y) (f (l x) y) :cost 0)
(rule (f x y) (f x (r y)) :cost 0)
(rule (l (el x)) (el (l x)) :cost 0)
(rule (el (r x)) (r (el x)) :cost 0)
