; @origtpdbfilename ./TRS/relative/rt2-8.trs
; @xtcfilename "./TRS_Relative/Relative_05/rt2-8.xml"
(format TRS)
(fun r 1)
(fun act 0)
(fun f 2)
(fun l 1)
(fun nact 0)
(fun el 1)
(rule (f act y) (f (el nact) y))
(rule (f x nact) (f x act))
(rule act (el nact))
(rule (l (el x)) (el (l x)))
(rule (el (r x)) (r (el x)))
(rule nact (l nact) :cost 0)
(rule nact (r nact) :cost 0)
