; @xtcfilename "./TRS_Conditional/COPS/351.xml"
(format CTRS oriented)
(fun ssp 2)
(fun nil 0)
(fun 0 0)
(fun sub 2)
(fun cons 2)
(fun s 1)
(rule (ssp (cons y |ys'|) v) xs (= (ssp |ys'| v) xs))
(rule (ssp (cons y |ys'|) v) (cons y |xs'|) (= (sub v y) w) (= (ssp |ys'| w) |xs'|))
(rule (sub (s v) (s w)) z (= (sub v w) z))
(rule (ssp nil 0) nil)
(rule (sub z 0) z)
