; @xtcfilename "./TRS_Conditional/COPS/365.xml"
(format CTRS oriented)
(fun s 1)
(fun p 1)
(fun pos 1)
(fun |0| 0)
(fun false 0)
(fun true 0)
(rule (pos (s x)) true (= (pos x) true))
(rule (pos (p x)) false (= (pos x) false))
(rule (s (p x)) x)
(rule (p (s x)) x)
(rule (pos |0|) false)
(rule (pos (s |0|)) true)
