; @xtcfilename "./TRS_Conditional/COPS/404.xml"
(format CTRS oriented)
(fun e 1)
(fun |0| 0)
(fun true 0)
(fun o 1)
(fun s 1)
(fun false 0)
(rule (e (s x)) true (= (o x) true))
(rule (e (s x)) false (= (e x) true))
(rule (o (s x)) true (= (e x) true))
(rule (o (s x)) false (= (o x) true))
(rule (e |0|) true)
(rule (o |0|) true)
