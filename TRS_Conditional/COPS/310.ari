; @xtcfilename "./TRS_Conditional/COPS/310.xml"
(format CTRS oriented)
(fun gcd 2)
(fun add 2)
(fun |0| 0)
(fun s 1)
(fun leq 2)
(fun false 0)
(rule (gcd x y) (gcd y x) (= (leq y x) false))
(rule (gcd (add x y) y) (gcd x y))
(rule (gcd y (add x y)) (gcd x y))
(rule (gcd x |0|) x)
(rule (gcd |0| x) x)
(rule (add |0| y) y)
(rule (add (s x) y) (s (add x y)))
