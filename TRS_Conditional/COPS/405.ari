; @xtcfilename "./TRS_Conditional/COPS/405.xml"
(format CTRS oriented)
(fun les 2)
(fun 0 0)
(fun false 0)
(fun s 1)
(fun true 0)
(fun dot 2)
(rule (dot x (dot y l)) (dot y (dot x l)) (= (les x y) true))
(rule (les 0 0) false)
(rule (les 0 (s 0)) true)
(rule (les 0 (s (s x))) (les 0 (s x)))
(rule (les (s 0) 0) false)
(rule (les (s (s x)) 0) (les (s x) 0))
(rule (les (s x) (s y)) (les x y))
