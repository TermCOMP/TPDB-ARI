; @xtcfilename "./TRS_Conditional/COPS/332.xml"
(format CTRS oriented)
(fun even 1)
(fun 0 0)
(fun true 0)
(fun odd 1)
(fun false 0)
(fun s 1)
(rule (even (s x)) false (= (odd x) false))
(rule (even (s x)) true (= (odd x) true))
(rule (odd (s x)) false (= (even x) false))
(rule (odd (s x)) true (= (even x) true))
(rule (even 0) true)
(rule (odd 0) false)
