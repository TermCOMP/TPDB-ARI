; @xtcfilename "./TRS_Conditional/COPS/292.xml"
(format CTRS oriented)
(fun le 2)
(fun 0 0)
(fun s 1)
(fun true 0)
(fun false 0)
(fun min 1)
(fun cons 2)
(fun nil 0)
(rule (min (cons x l)) x (= (le x (min l)) true))
(rule (min (cons x l)) (min l) (= (le x (min l)) false))
(rule (min (cons x l)) (min l) (= (min l) x))
(rule (le 0 (s x)) true)
(rule (le x 0) false)
(rule (le (s x) (s y)) (le x y))
(rule (min (cons x nil)) x)
