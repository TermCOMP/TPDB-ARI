; @xtcfilename "./TRS_Conditional/COPS/312.xml"
(format CTRS oriented)
(fun gcd 2)
(fun 0 0)
(fun iadd 1)
(fun tp2 2)
(fun s 1)
(fun u1 1)
(fun add 2)
(rule (gcd z y) (gcd x y) (= (iadd z) (tp2 x y)))
(rule (gcd y z) (gcd x y) (= (iadd z) (tp2 x y)))
(rule (gcd x 0) x)
(rule (gcd 0 x) x)
(rule (iadd y) (tp2 0 y))
(rule (iadd (s z)) (u1 (iadd z)))
(rule (u1 (tp2 x y)) (tp2 (s x) y))
(rule (iadd (add x y)) (tp2 x y))
