; @xtcfilename "./TRS_Conditional/Mixed_CTRS_2014/ex1-lucmes-wrla14.xml"
(format CTRS oriented)
(fun less 2)
(fun |0| 0)
(fun false 0)
(fun s 1)
(fun true 0)
(fun d 2)
(fun gcd 2)
(rule (gcd n m) (gcd (d n m) m) (= (less m |0|) true) (= (less n m) true))
(rule (less |0| m) false)
(rule (less (s n) |0|) true)
(rule (less (s n) (s m)) (less n m))
(rule (d n |0|) n)
(rule (d (s n) (s m)) (d n m))
(rule (gcd n |0|) |0|)
(rule (gcd n n) n)
