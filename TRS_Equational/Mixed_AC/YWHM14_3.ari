; @xtcfilename "./TRS_Equational/Mixed_AC/YWHM14_3.xml"
(format ETRS)
(fun f 1)
(fun plus 2 :theory AC)
(fun g 1)
(fun a 0)
(fun b 0)
(fun h 2)
(rule (f (plus x y)) (plus (f x) y))
(rule (plus (g x) y) (g (plus x y)))
(rule (plus (f a) (g b)) (plus (f b) (g a)))
(rule (h a b) (h b a))
(rule (h a (g (g a))) (h (g a) (f a)))
(rule (h (g a) a) (h a (g b)))
(rule (h (g a) b) (h a (g a)))
