(rule (|max'| 0 x) x)
(rule (|max'| (s x) (s y)) (s (|max'| x y)))
(rule (app empty X) X)
(rule (max (singl x)) x)
(rule (max (app (singl x) Y)) (max2 x Y))
(rule (max2 x empty) x)
(rule (max2 x (singl y)) (|max'| x y))
(rule (max2 x (app (singl y) Z)) (max2 (|max'| x y) Z))
