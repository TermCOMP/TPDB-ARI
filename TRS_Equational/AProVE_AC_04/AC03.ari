; @origtpdbfilename ./TRS/AProVE_AC/AC03.trs
; @xtcfilename "./TRS_Equational/AProVE_AC_04/AC03.xml"
(format ETRS)
(fun plus 2 :theory AC)
(fun 0 0)
(fun s 1)
(fun times 2 :theory AC)
(fun power 2)
(rule (plus x 0) x)
(rule (plus x (s y)) (s (plus x y)))
(rule (times x 0) 0)
(rule (times x (s y)) (plus x (times x y)))
(rule (power x 0) (s 0))
(rule (power x (s y)) (times x (power x y)))
