; @origtpdbfilename ./TRS/AProVE_AC/AC41.trs
; @xtcfilename "./TRS_Equational/AProVE_AC_04/AC41.xml"
(format ETRS)
(fun minus 2)
(fun 0 0)
(fun s 1)
(fun plus 2 :theory AC)
(fun quot 2)
(rule (minus x 0) x)
(rule (minus (s x) (s y)) (minus x y))
(rule (minus (minus x y) z) (minus x (plus y z)))
(rule (quot 0 (s y)) 0)
(rule (quot (s x) (s y)) (s (quot (minus x y) (s y))))
(rule (plus 0 y) y)
(rule (plus (s x) y) (s (plus x y)))
