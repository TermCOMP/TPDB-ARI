(format ETRS)
(fun plus 2 :theory AC)
(fun |0| 0)
(fun s 1)
(fun times 2 :theory AC)
(rule (plus x |0|) x)
(rule (plus x (s y)) (s (plus x y)))
(rule (plus x (plus (s y) z)) (plus (s (plus x y)) z))
(rule (times x |0|) |0|)
(rule (times x (times |0| z)) (times |0| z))
(rule (times x (s y)) (plus (times x y) x))
(rule (times x (times (s y) z)) (times (plus (times x y) x) z))
