; @origtpdbfilename ./TRS/AProVE_AC/AC48.trs
; @xtcfilename "./TRS_Equational/AProVE_AC_04/AC48.xml"
(format ETRS)
(fun times 2 :theory AC)
(fun 0 0)
(fun s 1)
(fun plus 2 :theory AC)
(rule (times x 0) 0)
(rule (times x (s y)) (plus (times x y) x))
(rule (plus x 0) x)
(rule (plus 0 x) x)
(rule (plus x (s y)) (s (plus x y)))
(rule (plus (s x) y) (s (plus x y)))
