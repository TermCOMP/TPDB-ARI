; @origtpdbfilename ./TRS/AProVE_AC/AC24.trs
; @xtcfilename "./TRS_Equational/AProVE_AC_04/AC24.xml"
(format ETRS)
(fun 0 1)
(fun S 0)
(fun plus 2 :theory AC)
(fun 1 1)
(fun times 2 :theory AC)
(rule (0 S) S)
(rule (plus S x) x)
(rule (plus (0 x) (0 y)) (0 (plus x y)))
(rule (plus (0 x) (1 y)) (1 (plus x y)))
(rule (plus (1 x) (1 y)) (0 (plus x (plus y (1 S)))))
(rule (times S x) S)
(rule (times (0 x) y) (0 (times x y)))
(rule (times (1 x) y) (plus (0 (times x y)) y))
