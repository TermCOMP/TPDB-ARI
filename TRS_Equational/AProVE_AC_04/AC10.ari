; @origtpdbfilename ./TRS/AProVE_AC/AC10.trs
; @xtcfilename "./TRS_Equational/AProVE_AC_04/AC10.xml"
(format ETRS)
(fun plus 2 :theory AC)
(fun 0 0)
(fun s 1)
(fun times 2 :theory AC)
(rule (plus x 0) x)
(rule (plus (s x) y) (s (plus x y)))
(rule (times x 0) 0)
(rule (times (s x) y) (plus (times x y) y))
(rule (times x (plus y z)) (plus (times x y) (times x z)))
