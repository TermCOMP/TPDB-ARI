(format TRS)
(fun app 2)
(fun quot 0)
(fun |0| 0)
(fun s 0)
(fun map 0)
(fun nil 0)
(fun cons 0)
(fun filter 0)
(fun filter2 0)
(fun true 0)
(fun false 0)
(rule (app (app (app quot |0|) (app s y)) (app s z)) |0|)
(rule (app (app (app quot (app s x)) (app s y)) z) (app (app (app quot x) y) z))
