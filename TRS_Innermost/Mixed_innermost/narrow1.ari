; @xtcfilename "./TRS_Innermost/Mixed_innermost/narrow1.xml"
(format TRS)
(fun ack 2)
(fun |0| 0)
(fun s 1)
(fun init 0)
(fun cc 2)
(fun f 1)
(fun aa 0)
(rule (ack |0| y) (s y))
(rule (ack (s x) |0|) (ack x (s |0|)))
(rule (ack (s x) (s y)) (ack x (ack (s x) y)))
(rule init (ack (s (s (s (s (s (s (s |0|))))))) |0|))
(rule (cc x y) x)
(rule (cc x y) y)
(rule (f aa) (f (cc aa init)))
