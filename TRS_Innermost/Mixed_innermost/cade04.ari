; @origtpdbfilename ./TRS/SchneiderKamp/inn/cade04.trs
; @xtcfilename "./TRS_Innermost/Mixed_innermost/cade04.xml"
(format TRS)
(fun minus 2)
(fun |0| 0)
(fun cond 3)
(fun min 2)
(fun s 1)
(rule (minus x x) |0|)
(rule (minus x y) (cond (min x y) x y))
(rule (cond y x y) (s (minus x (s y))))
(rule (min |0| v) |0|)
(rule (min u |0|) |0|)
(rule (min (s u) (s v)) (s (min u v)))
