; @xtcfilename "./TRS_Innermost/Mixed_innermost/innermost5.xml"
(format TRS)
(fun f 3)
(fun s 1)
(fun b 0)
(fun g 1)
(fun h 1)
(fun i 1)
(fun 0 0)
(fun a 0)
(rule (f (s x) y b) (f (g (h x)) y (i y)))
(rule (g (h x)) (g x))
(rule (g (s x)) (s x))
(rule (g 0) (s 0))
(rule (h 0) a)
(rule (i 0) b)
(rule (i (s y)) (i y))
